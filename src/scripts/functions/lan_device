		let uci_num=$num-1
		set_wlan_basic_encryption_modes $uci_num "$2"
	fi
	return $FAULT_CPE_NO_FAULT
	;;
	InternetGatewayDevice.LANDevice.1.WLANConfiguration.[1-$max_num].BasicAuthenticationMode)
	freecwmp_parse_formated_parameter "$1" "InternetGatewayDevice.LANDevice.1.WLANConfiguration.{i}.BasicAuthenticationMode" "rc" "num"
