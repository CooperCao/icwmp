local val="$2"
case $action in
	set_value)
	delay_command "wifi" "wifi" "45"
	/sbin/uci ${UCI_CONFIG_DIR:+-c $UCI_CONFIG_DIR} -q set wireless.@wifi-iface[$num].ssid="$val"
	;;
	set_notification)
	let num=$num+1
	local parm="InternetGatewayDevice.LANDevice.1.WLANConfiguration.$num.SSID"
	freecwmp_set_parameter_notification "$parm" "$val"
	;;
