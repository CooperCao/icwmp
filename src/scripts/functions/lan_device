get_wlan_pre_shared_key_key_passphrase() {
local num="$1"
local key_index="$2"
local val=""
local permissions=""
# this parameter must an empty value (TR-098 standard)
let num=$num+1
case "$action" in
	get_value)
	freecwmp_output "InternetGatewayDevice.LANDevice.1.WLANConfiguration.$num.PreSharedKey.$key_index.KeyPassphrase" "$val" "$permissions"
	;;
	get_name)
	permissions="1"
	freecwmp_output "InternetGatewayDevice.LANDevice.1.WLANConfiguration.$num.PreSharedKey.$key_index.KeyPassphrase" "" "$permissions"
	;;
	get_notification)
	freecwmp_get_parameter_notification "val" "InternetGatewayDevice.LANDevice.1.WLANConfiguration.$num.KeyPassphrase.$key_index.PreSharedKey"
	freecwmp_output "InternetGatewayDevice.LANDevice.1.WLANConfiguration.$num.PreSharedKey.$key_index.KeyPassphrase" "$val"
	;;
esac
}

set_wlan_pre_shared_key_key_passphrase() {
local num="$1"
local key_index="$2"
local val="$3"
case $action in
	set_value)
	delay_command "wifi" "wifi" "45"
	let num=$num+1
	local parm="InternetGatewayDevice.LANDevice.1.WLANConfiguration.$num.PreSharedKey.$key_index.KeyPassphrase"
	freecwmp_notify "$parm" "$2" "xsd:string"
	;;
	set_notification)
	let num=$num+1
	local parm="InternetGatewayDevice.LANDevice.1.WLANConfiguration.$num.PreSharedKey.$key_index.KeyPassphrase"
	freecwmp_set_parameter_notification "$parm" "$val"
	;;
esac
}

get_wlan_pre_shared_key_associated_device_MACAddress() {
local num="$1"
local key_index="$2"
local val=""
local permissions=""
# this parameter must an empty value (TR-098 standard)
let num=$num+1
case "$action" in
	get_value)
	freecwmp_output "InternetGatewayDevice.LANDevice.1.WLANConfiguration.$num.PreSharedKey.$key_index.AssociatedDeviceMACAddress" "$val" "$permissions"
	;;
	get_name)
	permissions="1"
	freecwmp_output "InternetGatewayDevice.LANDevice.1.WLANConfiguration.$num.PreSharedKey.$key_index.AssociatedDeviceMACAddress" "" "$permissions"
	;;
	get_notification)
	freecwmp_get_parameter_notification "val" "InternetGatewayDevice.LANDevice.1.WLANConfiguration.$num.PreSharedKey.$key_index.AssociatedDeviceMACAddress"
	freecwmp_output "InternetGatewayDevice.LANDevice.1.WLANConfiguration.$num.PreSharedKey.$key_index.AssociatedDeviceMACAddress" "$val"
	;;
esac
}

set_wlan_pre_shared_key_associated_device_MACAddress() {
local num="$1"
local key_index="$2"
local val="$3"
case $action in
	set_value)
	delay_command "wifi" "wifi" "45"
	let num=$num+1
	local parm="InternetGatewayDevice.LANDevice.1.WLANConfiguration.$num.PreSharedKey.$key_index.AssociatedDeviceMACAddress"
	freecwmp_notify "$parm" "$2" "xsd:string"
	;;
	set_notification)
	let num=$num+1
	local parm="InternetGatewayDevice.LANDevice.1.WLANConfiguration.$num.PreSharedKey.$key_index.AssociatedDeviceMACAddress"
	freecwmp_set_parameter_notification "$parm" "$val"
	;;
esac
}

add_ip_interface() {
	local i=0
	local instance=0
	while [ "`/sbin/uci ${UCI_CONFIG_DIR:+-c $UCI_CONFIG_DIR} -q get cwmp.@object[$i].instance 2>/dev/null`" != "" ] ;do
		instance=`/sbin/uci ${UCI_CONFIG_DIR:+-c $UCI_CONFIG_DIR} -q get cwmp.@object[$i].instance 2>/dev/null`
		let i=$i+1
	done
	let instance=$instance+1
	freecwmp_add_lan_device_ip_interface "$1$instance." "$instance"
	/sbin/uci ${UCI_CONFIG_DIR:+-c $UCI_CONFIG_DIR} -q commit
	freecwmp_output "" "" "" "" "" "0" "$instance"
}

get_ip_interface() {
local i=0
while [ "`/sbin/uci ${UCI_CONFIG_DIR:+-c $UCI_CONFIG_DIR} get cwmp.@object[$i].parameter 2>/dev/null`" != "" ]
do
	if [ "$action" = "get_name" ];then
			let i=$i+1
			local parm=`/sbin/uci ${UCI_CONFIG_DIR:+-c $UCI_CONFIG_DIR} -q get cwmp.@object[$i].parameter 2>/dev/null`
			permissions="1"
			freecwmp_output "$parm" "" "$permissions"
	fi
done
}

get_ip_interface_ip_address() {
local val=""
local permissions=""
local i=0
while [ "`/sbin/uci ${UCI_CONFIG_DIR:+-c $UCI_CONFIG_DIR} -q get cwmp.@object[$i].parameter 2>/dev/null`" != "" ]
do
	local parm=`/sbin/uci ${UCI_CONFIG_DIR:+-c $UCI_CONFIG_DIR} -q get cwmp.@object[$i].parameter 2>/dev/null`
	local indice=`/sbin/uci ${UCI_CONFIG_DIR:+-c $UCI_CONFIG_DIR} -q get cwmp.@object[$i].instance 2>/dev/null`
	case "$action" in
		get_value)
		val=`/sbin/uci ${UCI_CONFIG_DIR:+-c $UCI_CONFIG_DIR} -q get network.lan$indice.ipaddr 2> /dev/null`
		freecwmp_output $parm"IPInterfaceIPAddress" "$val"
		;;
		get_name)
		permissions="1"
		freecwmp_output $parm"IPInterfaceIPAddress" "" "$permissions"
		;;
	esac
	let i=$i+1
done
}

set_ip_interface_ip_address() {
	local i=0
	local indice
	local _parm="$1"
	local ipaddr="$2"
	_parm="${_parm%.*}."
	while [ \( "`/sbin/uci ${UCI_CONFIG_DIR:+-c $UCI_CONFIG_DIR} -q get cwmp.@object[$i].parameter 2>/dev/null`" != "$_parm" \) -a \( "`/sbin/uci ${UCI_CONFIG_DIR:+-c $UCI_CONFIG_DIR} -q get cwmp.@object[$i].parameter 2>/dev/null`" != "" \) ]
	do
		let i=$i+1
	done
	if [ "`/sbin/uci ${UCI_CONFIG_DIR:+-c $UCI_CONFIG_DIR} -q get cwmp.@object[$i].parameter 2>/dev/null`" = "" ]; then
		return $FAULT_CPE_INVALID_PARAMETER_NAME
	fi
	local indice=`/sbin/uci ${UCI_CONFIG_DIR:+-c $UCI_CONFIG_DIR} -q get cwmp.@object[$i].instance`
	/sbin/uci ${UCI_CONFIG_DIR:+-c $UCI_CONFIG_DIR} -q set network.lan$indice.ipaddr=$ipaddr
	/sbin/uci ${UCI_CONFIG_DIR:+-c $UCI_CONFIG_DIR} -q set network.lan$indice.proto=static
	/sbin/uci ${UCI_CONFIG_DIR:+-c $UCI_CONFIG_DIR} -q commit
	return $FAULT_CPE_NO_FAULT
}

get_ip_interface_netmask() {
local val=""
local permissions=""
local i=0
while [ "`/sbin/uci ${UCI_CONFIG_DIR:+-c $UCI_CONFIG_DIR} -q get cwmp.@object[$i].parameter 2>/dev/null`" != "" ]
do
	local parm=`/sbin/uci ${UCI_CONFIG_DIR:+-c $UCI_CONFIG_DIR} -q get cwmp.@object[$i].parameter 2>/dev/null`
	local indice=`/sbin/uci ${UCI_CONFIG_DIR:+-c $UCI_CONFIG_DIR} -q get cwmp.@object[$i].instance 2>/dev/null`
	case "$action" in
		get_value)
		val=`/sbin/uci ${UCI_CONFIG_DIR:+-c $UCI_CONFIG_DIR} -q get network.lan$indice.netmask 2> /dev/null`
		freecwmp_output $parm"IPInterfaceSubnetMask" "$val"
		;;
		get_name)
		permissions="1"
		freecwmp_output $parm"IPInterfaceSubnetMask" "" "$permissions"
		;;
	esac
	let i=$i+1
done
}

set_ip_interface_netmask() {
	local i=0
	local indice
	local _parm="$1"
	local netmask="$2"
	_parm="${_parm%.*}."
	while [ \( "`/sbin/uci ${UCI_CONFIG_DIR:+-c $UCI_CONFIG_DIR} -q get cwmp.@object[$i].parameter 2>/dev/null`" != "$_parm" \) -a \( "`/sbin/uci ${UCI_CONFIG_DIR:+-c $UCI_CONFIG_DIR} -q get cwmp.@object[$i].parameter 2>/dev/null`" != "" \) ]
	do
		let i=$i+1
	done
	if [ "`/sbin/uci ${UCI_CONFIG_DIR:+-c $UCI_CONFIG_DIR} -q get cwmp.@object[$i].parameter 2>/dev/null`" = "" ]; then
		return $FAULT_CPE_INVALID_PARAMETER_NAME
	fi
	local indice=`/sbin/uci ${UCI_CONFIG_DIR:+-c $UCI_CONFIG_DIR} -q get cwmp.@object[$i].instance`
	/sbin/uci ${UCI_CONFIG_DIR:+-c $UCI_CONFIG_DIR} -q set network.lan$indice.netmask=$netmask
	/sbin/uci ${UCI_CONFIG_DIR:+-c $UCI_CONFIG_DIR} -q commit
	return $FAULT_CPE_NO_FAULT
}

delete_ip_interface() {
	local indice=`/sbin/uci ${UCI_CONFIG_DIR:+-c $UCI_CONFIG_DIR} -q get cwmp.@object[$1].instance`
	/sbin/uci ${UCI_CONFIG_DIR:+-c $UCI_CONFIG_DIR} -q delete cwmp.@object[$1]
	/sbin/uci ${UCI_CONFIG_DIR:+-c $UCI_CONFIG_DIR} -q delete network.lan$indice
	/sbin/uci ${UCI_CONFIG_DIR:+-c $UCI_CONFIG_DIR} -q commit
	freecwmp_output "" "" "" "" "" "0"
}

add_lan_device() {
case "$1" in
	InternetGatewayDevice.LANDevice.1.LANHostConfigManagement.IPInterface.)
	add_ip_interface $1
	return $FAULT_CPE_NO_FAULT
	;;
esac
return $FAULT_CPE_INVALID_PARAMETER_NAME
}

delete_lan_device() {
local _parm="$1"
local i=0
while [ \( "`/sbin/uci ${UCI_CONFIG_DIR:+-c $UCI_CONFIG_DIR} -q get cwmp.@object[$i].parameter 2>/dev/null`" != "$_parm" \) -a \( "`/sbin/uci ${UCI_CONFIG_DIR:+-c $UCI_CONFIG_DIR} -q get cwmp.@object[$i].parameter 2>/dev/null`" != "" \) ]
do
	let i=$i+1
done
if [ "`/sbin/uci ${UCI_CONFIG_DIR:+-c $UCI_CONFIG_DIR} -q get cwmp.@object[$i].parameter 2>/dev/null`" = "" ]; then
	return $FAULT_CPE_INVALID_PARAMETER_NAME
fi
case "${_parm%.*.}." in
	InternetGatewayDevice.LANDevice.1.LANHostConfigManagement.IPInterface.)
	delete_ip_interface $i
	return $FAULT_CPE_NO_FAULT
	;;
esac
}

get_lan_device() {
local max_num

get_wlan_number_of_entries "max_num"

case "$1" in
	InternetGatewayDevice.)
	get_ip_interface_ip_address
	get_ip_interface_netmask
	for i in `seq 1 $max_num`;do
		freecwmp_parse_formated_parameter $1"LANDevice.1.WLANConfiguration.$i." "InternetGatewayDevice.LANDevice.1.WLANConfiguration.{i}." "rc" "num"
		if [ $rc -eq 0 ] && [ $num -gt 0 ] && [ $num -le $max_num ]; then
			let uci_num=$num-1
			get_wlan_enable $uci_num
			get_wlan_status $uci_num
			get_wlan_bssid $uci_num
			get_wlan_max_bit_rate $uci_num
			get_wlan_channel $uci_num
			get_wlan_ssid $uci_num
			get_wlan_beacon_type $uci_num
			get_wlan_mac_control_enable $uci_num
			get_wlan_standard $uci_num
			get_wlan_wep_key_index $uci_num
			get_wlan_key_passphrase $uci_num
			get_wlan_wep_encryption_level $uci_num
			get_wlan_basic_encryption_modes $uci_num
			get_wlan_basic_authentication_mode $uci_num
			get_wlan_wpa_encryption_modes $uci_num
			get_wlan_wpa_authentication_mode $uci_num
			get_wlan_ieee_11i_encryption_modes $uci_num
			get_wlan_ieee_11i_authentication_mode $uci_num
			get_wlan_radio_enabled $uci_num
			get_wlan_device_operation_mode $uci_num
			get_wlan_authentication_service_mode $uci_num
			get_wlan_total_associations $uci_num
			get_wlan_associated_device $uci_num
			get_wlan_wep_key $uci_num "1"
			get_wlan_wep_key $uci_num "2"
			get_wlan_wep_key $uci_num "3"
			get_wlan_wep_key $uci_num "4"
			get_wlan_pre_shared_key $uci_num "1"
			get_wlan_pre_shared_key_key_passphrase $uci_num "1"
			get_wlan_pre_shared_key_associated_device_MACAddress $uci_num "1"
		fi
	done
	return $FAULT_CPE_NO_FAULT
	;;
	InternetGatewayDevice.LANDevice.)
	get_ip_interface_ip_address
	get_ip_interface_netmask
	for i in `seq 1 $max_num`;do
		freecwmp_parse_formated_parameter $1"1.WLANConfiguration.$i." "InternetGatewayDevice.LANDevice.1.WLANConfiguration.{i}." "rc" "num"
		if [ $rc -eq 0 ] && [ $num -gt 0 ] && [ $num -le $max_num ]; then
			let uci_num=$num-1
			get_wlan_enable $uci_num
			get_wlan_status $uci_num
			get_wlan_bssid $uci_num
			get_wlan_max_bit_rate $uci_num
			get_wlan_channel $uci_num
			get_wlan_ssid $uci_num
			get_wlan_beacon_type $uci_num
			get_wlan_mac_control_enable $uci_num
			get_wlan_standard $uci_num
			get_wlan_wep_key_index $uci_num
			get_wlan_key_passphrase $uci_num
			get_wlan_wep_encryption_level $uci_num
			get_wlan_basic_encryption_modes $uci_num
			get_wlan_basic_authentication_mode $uci_num
			get_wlan_wpa_encryption_modes $uci_num
			get_wlan_wpa_authentication_mode $uci_num
			get_wlan_ieee_11i_encryption_modes $uci_num
			get_wlan_ieee_11i_authentication_mode $uci_num
			get_wlan_radio_enabled $uci_num
			get_wlan_device_operation_mode $uci_num
			get_wlan_authentication_service_mode $uci_num
			get_wlan_total_associations $uci_num
			get_wlan_associated_device $uci_num
			get_wlan_wep_key $uci_num "1"
			get_wlan_wep_key $uci_num "2"
			get_wlan_wep_key $uci_num "3"
			get_wlan_wep_key $uci_num "4"
			get_wlan_pre_shared_key $uci_num "1"
			get_wlan_pre_shared_key_key_passphrase $uci_num "1"
			get_wlan_pre_shared_key_associated_device_MACAddress $uci_num "1"
		fi
	done
	return $FAULT_CPE_NO_FAULT
	;;
	InternetGatewayDevice.LANDevice.1.)
	get_ip_interface_ip_address
	get_ip_interface_netmask
	for i in `seq 1 $max_num`;do
		freecwmp_parse_formated_parameter $1"WLANConfiguration.$i." "InternetGatewayDevice.LANDevice.1.WLANConfiguration.{i}." "rc" "num"
		if [ $rc -eq 0 ] && [ $num -gt 0 ] && [ $num -le $max_num ]; then
			let uci_num=$num-1
			get_wlan_enable $uci_num
			get_wlan_status $uci_num
			get_wlan_bssid $uci_num
			get_wlan_max_bit_rate $uci_num
			get_wlan_channel $uci_num
			get_wlan_ssid $uci_num
			get_wlan_beacon_type $uci_num
			get_wlan_mac_control_enable $uci_num
			get_wlan_standard $uci_num
			get_wlan_wep_key_index $uci_num
			get_wlan_key_passphrase $uci_num
			get_wlan_wep_encryption_level $uci_num
			get_wlan_basic_encryption_modes $uci_num
			get_wlan_basic_authentication_mode $uci_num
			get_wlan_wpa_encryption_modes $uci_num
			get_wlan_wpa_authentication_mode $uci_num
			get_wlan_ieee_11i_encryption_modes $uci_num
			get_wlan_ieee_11i_authentication_mode $uci_num
			get_wlan_radio_enabled $uci_num
			get_wlan_device_operation_mode $uci_num
			get_wlan_authentication_service_mode $uci_num
			get_wlan_total_associations $uci_num
			get_wlan_associated_device $uci_num
			get_wlan_wep_key $uci_num "1"
			get_wlan_wep_key $uci_num "2"
			get_wlan_wep_key $uci_num "3"
			get_wlan_wep_key $uci_num "4"
			get_wlan_pre_shared_key $uci_num "1"
			get_wlan_pre_shared_key_key_passphrase $uci_num "1"
			get_wlan_pre_shared_key_associated_device_MACAddress $uci_num "1"
		fi
	done
	return $FAULT_CPE_NO_FAULT
	;;
	InternetGatewayDevice.LANDevice.1.LANHostConfigManagement.)
	get_ip_interface_ip_address
	get_ip_interface_netmask
	return $FAULT_CPE_NO_FAULT
	;;
	InternetGatewayDevice.LANDevice.1.LANHostConfigManagement.IPInterface.)
	get_ip_interface_ip_address
	get_ip_interface_netmask
	return $FAULT_CPE_NO_FAULT
	;;
	InternetGatewayDevice.LANDevice.1.LANHostConfigManagement.IPInterface.[1-9]+.)
	get_ip_interface_ip_address
	get_ip_interface_netmask
	return $FAULT_CPE_NO_FAULT
	;;
	InternetGatewayDevice.LANDevice.1.WLANConfiguration.)
	for i in `seq 1 $max_num`;do
		freecwmp_parse_formated_parameter "$1$i." "InternetGatewayDevice.LANDevice.1.WLANConfiguration.{i}." "rc" "num"
		if [ $rc -eq 0 ] && [ $num -gt 0 ] && [ $num -le $max_num ]; then
			let uci_num=$num-1
			get_wlan_enable $uci_num
			get_wlan_status $uci_num
			get_wlan_bssid $uci_num
			get_wlan_max_bit_rate $uci_num
			get_wlan_channel $uci_num
			get_wlan_ssid $uci_num
			get_wlan_beacon_type $uci_num
			get_wlan_mac_control_enable $uci_num
			get_wlan_standard $uci_num
			get_wlan_wep_key_index $uci_num
			get_wlan_key_passphrase $uci_num
			get_wlan_wep_encryption_level $uci_num
			get_wlan_basic_encryption_modes $uci_num
			get_wlan_basic_authentication_mode $uci_num
			get_wlan_wpa_encryption_modes $uci_num
			get_wlan_wpa_authentication_mode $uci_num
			get_wlan_ieee_11i_encryption_modes $uci_num
			get_wlan_ieee_11i_authentication_mode $uci_num
			get_wlan_radio_enabled $uci_num
			get_wlan_device_operation_mode $uci_num
			get_wlan_authentication_service_mode $uci_num
			get_wlan_total_associations $uci_num
			get_wlan_associated_device $uci_num
			get_wlan_wep_key $uci_num "1"
			get_wlan_wep_key $uci_num "2"
			get_wlan_wep_key $uci_num "3"
			get_wlan_wep_key $uci_num "4"
			get_wlan_pre_shared_key $uci_num "1"
			get_wlan_pre_shared_key_key_passphrase $uci_num "1"
			get_wlan_pre_shared_key_associated_device_MACAddress $uci_num "1"
		fi
	done
	return $FAULT_CPE_NO_FAULT
	;;
	InternetGatewayDevice.LANDevice.1.WLANConfiguration.[1-$max_num].)
	freecwmp_parse_formated_parameter "$1" "InternetGatewayDevice.LANDevice.1.WLANConfiguration.{i}." "rc" "num"
	if [ $rc -eq 0 ] && [ $num -gt 0 ] && [ $num -le $max_num ]; then
		let uci_num=$num-1
		get_wlan_enable $uci_num
		get_wlan_status $uci_num
		get_wlan_bssid $uci_num
		get_wlan_max_bit_rate $uci_num
		get_wlan_channel $uci_num
		get_wlan_ssid $uci_num
		get_wlan_beacon_type $uci_num
		get_wlan_mac_control_enable $uci_num
		get_wlan_standard $uci_num
		get_wlan_wep_key_index $uci_num
		get_wlan_key_passphrase $uci_num
		get_wlan_wep_encryption_level $uci_num
		get_wlan_basic_encryption_modes $uci_num
		get_wlan_basic_authentication_mode $uci_num
		get_wlan_wpa_encryption_modes $uci_num
		get_wlan_wpa_authentication_mode $uci_num
		get_wlan_ieee_11i_encryption_modes $uci_num
		get_wlan_ieee_11i_authentication_mode $uci_num
		get_wlan_radio_enabled $uci_num
		get_wlan_device_operation_mode $uci_num
		get_wlan_authentication_service_mode $uci_num
		get_wlan_total_associations $uci_num
		get_wlan_associated_device $uci_num
		get_wlan_wep_key $uci_num "1"
		get_wlan_wep_key $uci_num "2"
		get_wlan_wep_key $uci_num "3"
		get_wlan_wep_key $uci_num "4"
		get_wlan_pre_shared_key $uci_num "1"
		get_wlan_pre_shared_key_key_passphrase $uci_num "1"
		get_wlan_pre_shared_key_associated_device_MACAddress $uci_num "1"
	fi
	return $FAULT_CPE_NO_FAULT
	;;
	InternetGatewayDevice.LANDevice.1.WLANConfiguration.[1-$max_num].Enable)
	freecwmp_parse_formated_parameter "$1.$i." "InternetGatewayDevice.LANDevice.1.WLANConfiguration.{i}.Enable" "rc" "num"
	if [ $rc -eq 0 ] && [ $num -gt 0 ] && [ $num -le $max_num ]; then
		let uci_num=$num-1
		get_wlan_enable $uci_num
	fi
	return $FAULT_CPE_NO_FAULT
	;;
	InternetGatewayDevice.LANDevice.1.WLANConfiguration.[1-$max_num].Status)
	freecwmp_parse_formated_parameter "$1" "InternetGatewayDevice.LANDevice.1.WLANConfiguration.{i}.Status" "rc" "num"
	if [ $rc -eq 0 ] && [ $num -gt 0 ] && [ $num -le $max_num ]; then
		let uci_num=$num-1
		get_wlan_status $uci_num
	fi
	return $FAULT_CPE_NO_FAULT
	;;
	InternetGatewayDevice.LANDevice.1.WLANConfiguration.[1-$max_num].BSSID)
	freecwmp_parse_formated_parameter "$1" "InternetGatewayDevice.LANDevice.1.WLANConfiguration.{i}.BSSID" "rc" "num"
	if [ $rc -eq 0 ] && [ $num -gt 0 ] && [ $num -le $max_num ]; then
		let uci_num=$num-1
		get_wlan_bssid $uci_num
	fi
	return $FAULT_CPE_NO_FAULT
	;;	
	InternetGatewayDevice.LANDevice.1.WLANConfiguration.[1-$max_num].MaxBitRate)
	freecwmp_parse_formated_parameter "$1" "InternetGatewayDevice.LANDevice.1.WLANConfiguration.{i}.MaxBitRate" "rc" "num"
	if [ $rc -eq 0 ] && [ $num -gt 0 ] && [ $num -le $max_num ]; then
		let uci_num=$num-1
		get_wlan_max_bit_rate $uci_num
	fi
	return $FAULT_CPE_NO_FAULT
	;;
	InternetGatewayDevice.LANDevice.1.WLANConfiguration.[1-$max_num].Channel)
	freecwmp_parse_formated_parameter "$1" "InternetGatewayDevice.LANDevice.1.WLANConfiguration.{i}.Channel" "rc" "num"
	if [ $rc -eq 0 ] && [ $num -gt 0 ] && [ $num -le $max_num ]; then
		let uci_num=$num-1
		get_wlan_channel $uci_num
	fi
	return $FAULT_CPE_NO_FAULT
	;;
	InternetGatewayDevice.LANDevice.1.WLANConfiguration.[1-$max_num].SSID)
	freecwmp_parse_formated_parameter "$1" "InternetGatewayDevice.LANDevice.1.WLANConfiguration.{i}.SSID" "rc" "num"
	if [ $rc -eq 0 ] && [ $num -gt 0 ] && [ $num -le $max_num ]; then
		let uci_num=$num-1
		get_wlan_ssid $uci_num
	fi
	return $FAULT_CPE_NO_FAULT
	;;
	InternetGatewayDevice.LANDevice.1.WLANConfiguration.[1-$max_num].BeaconType)
	freecwmp_parse_formated_parameter "$1" "InternetGatewayDevice.LANDevice.1.WLANConfiguration.{i}.BeaconType" "rc" "num"
	if [ $rc -eq 0 ] && [ $num -gt 0 ] && [ $num -le $max_num ]; then
		let uci_num=$num-1
		get_wlan_beacon_type $uci_num
	fi
	return $FAULT_CPE_NO_FAULT
	;;
	InternetGatewayDevice.LANDevice.1.WLANConfiguration.[1-$max_num].MACAddressControlEnabled)
	freecwmp_parse_formated_parameter "$1" "InternetGatewayDevice.LANDevice.1.WLANConfiguration.{i}.MACAddressControlEnabled" "rc" "num"
	if [ $rc -eq 0 ] && [ $num -gt 0 ] && [ $num -le $max_num ]; then
		let uci_num=$num-1
		get_wlan_mac_control_enable $uci_num
	fi
	return $FAULT_CPE_NO_FAULT
	;;
	InternetGatewayDevice.LANDevice.1.WLANConfiguration.[1-$max_num].Standard)
	freecwmp_parse_formated_parameter "$1" "InternetGatewayDevice.LANDevice.1.WLANConfiguration.{i}.Standard" "rc" "num"
	if [ $rc -eq 0 ] && [ $num -gt 0 ] && [ $num -le $max_num ]; then
		let uci_num=$num-1
		get_wlan_standard $uci_num
	fi
	return $FAULT_CPE_NO_FAULT
	;;
	InternetGatewayDevice.LANDevice.1.WLANConfiguration.[1-$max_num].WEPKeyIndex)
	freecwmp_parse_formated_parameter "$1" "InternetGatewayDevice.LANDevice.1.WLANConfiguration.{i}.WEPKeyIndex" "rc" "num"
	if [ $rc -eq 0 ] && [ $num -gt 0 ] && [ $num -le $max_num ]; then
		let uci_num=$num-1
		get_wlan_wep_key_index $uci_num
	fi
	return $FAULT_CPE_NO_FAULT
	;;
	InternetGatewayDevice.LANDevice.1.WLANConfiguration.[1-$max_num].KeyPassphrase)
	freecwmp_parse_formated_parameter "$1" "InternetGatewayDevice.LANDevice.1.WLANConfiguration.{i}.KeyPassphrase" "rc" "num"
	if [ $rc -eq 0 ] && [ $num -gt 0 ] && [ $num -le $max_num ]; then
		let uci_num=$num-1
		get_wlan_key_passphrase $uci_num
	fi
	return $FAULT_CPE_NO_FAULT
	;;
	InternetGatewayDevice.LANDevice.1.WLANConfiguration.[1-$max_num].WEPEncryptionLevel)
	freecwmp_parse_formated_parameter "$1" "InternetGatewayDevice.LANDevice.1.WLANConfiguration.{i}.WEPEncryptionLevel" "rc" "num"
	if [ $rc -eq 0 ] && [ $num -gt 0 ] && [ $num -le $max_num ]; then
		let uci_num=$num-1
		get_wlan_wep_encryption_level $uci_num
	fi
	return $FAULT_CPE_NO_FAULT
	;;
	InternetGatewayDevice.LANDevice.1.WLANConfiguration.[1-$max_num].BasicEncryptionModes)
	freecwmp_parse_formated_parameter "$1" "InternetGatewayDevice.LANDevice.1.WLANConfiguration.{i}.BasicEncryptionModes" "rc" "num"
	if [ $rc -eq 0 ] && [ $num -gt 0 ] && [ $num -le $max_num ]; then
		let uci_num=$num-1
		get_wlan_basic_encryption_modes $uci_num
	fi
	return $FAULT_CPE_NO_FAULT
	;;
	InternetGatewayDevice.LANDevice.1.WLANConfiguration.[1-$max_num].BasicAuthenticationMode)
	freecwmp_parse_formated_parameter "$1" "InternetGatewayDevice.LANDevice.1.WLANConfiguration.{i}.BasicAuthenticationMode" "rc" "num"
	if [ $rc -eq 0 ] && [ $num -gt 0 ] && [ $num -le $max_num ]; then
		let uci_num=$num-1
		get_wlan_basic_authentication_mode $uci_num
	fi
	return $FAULT_CPE_NO_FAULT
	;;
	InternetGatewayDevice.LANDevice.1.WLANConfiguration.[1-$max_num].WPAEncryptionModes)
	freecwmp_parse_formated_parameter "$1" "InternetGatewayDevice.LANDevice.1.WLANConfiguration.{i}.WPAEncryptionModes" "rc" "num"
	if [ $rc -eq 0 ] && [ $num -gt 0 ] && [ $num -le $max_num ]; then
		let uci_num=$num-1
		get_wlan_wpa_encryption_modes $uci_num
	fi
	return $FAULT_CPE_NO_FAULT
	;;
	InternetGatewayDevice.LANDevice.1.WLANConfiguration.[1-$max_num].WPAAuthenticationMode)
	freecwmp_parse_formated_parameter "$1" "InternetGatewayDevice.LANDevice.1.WLANConfiguration.{i}.WPAAuthenticationMode" "rc" "num"
	if [ $rc -eq 0 ] && [ $num -gt 0 ] && [ $num -le $max_num ]; then
		let uci_num=$num-1
		get_wlan_wpa_authentication_mode $uci_num
	fi
	return $FAULT_CPE_NO_FAULT
	;;
	InternetGatewayDevice.LANDevice.1.WLANConfiguration.[1-$max_num].IEEE11iEncryptionModes)
	freecwmp_parse_formated_parameter "$1" "InternetGatewayDevice.LANDevice.1.WLANConfiguration.{i}.IEEE11iEncryptionModes" "rc" "num"
	if [ $rc -eq 0 ] && [ $num -gt 0 ] && [ $num -le $max_num ]; then
		let uci_num=$num-1
		get_wlan_ieee_11i_encryption_modes $uci_num
	fi
	return $FAULT_CPE_NO_FAULT
	;;
	InternetGatewayDevice.LANDevice.1.WLANConfiguration.[1-$max_num].IEEE11iAuthenticationMode)
	freecwmp_parse_formated_parameter "$1" "InternetGatewayDevice.LANDevice.1.WLANConfiguration.{i}.IEEE11iAuthenticationMode" "rc" "num"
	if [ $rc -eq 0 ] && [ $num -gt 0 ] && [ $num -le $max_num ]; then
		let uci_num=$num-1
		get_wlan_ieee_11i_authentication_mode $uci_num
	fi
	return $FAULT_CPE_NO_FAULT
	;;	
	InternetGatewayDevice.LANDevice.1.WLANConfiguration.[1-$max_num].DeviceOperationMode)
	freecwmp_parse_formated_parameter "$1" "InternetGatewayDevice.LANDevice.1.WLANConfiguration.{i}.DeviceOperationMode" "rc" "num"
	if [ $rc -eq 0 ] && [ $num -gt 0 ] && [ $num -le $max_num ]; then
		let uci_num=$num-1
		get_wlan_device_operation_mode $uci_num
	fi
	return $FAULT_CPE_NO_FAULT
	;;	
	InternetGatewayDevice.LANDevice.1.WLANConfiguration.[1-$max_num].AuthenticationServiceMode)
	freecwmp_parse_formated_parameter "$1" "InternetGatewayDevice.LANDevice.1.WLANConfiguration.{i}.AuthenticationServiceMode" "rc" "num"
	if [ $rc -eq 0 ] && [ $num -gt 0 ] && [ $num -le $max_num ]; then
		let uci_num=$num-1
		get_wlan_authentication_service_mode $uci_num
	fi
	return $FAULT_CPE_NO_FAULT
	;;
	InternetGatewayDevice.LANDevice.1.WLANConfiguration.[1-$max_num].RadioEnabled)
	freecwmp_parse_formated_parameter "$1" "InternetGatewayDevice.LANDevice.1.WLANConfiguration.{i}.RadioEnabled" "rc" "num"
	if [ $rc -eq 0 ] && [ $num -gt 0 ] && [ $num -le $max_num ]; then
		let uci_num=$num-1
		get_wlan_radio_enabled $uci_num
	fi
	return $FAULT_CPE_NO_FAULT
	;;
	InternetGatewayDevice.LANDevice.1.WLANConfiguration.[1-$max_num].TotalAssociations)
	freecwmp_parse_formated_parameter "$1" "InternetGatewayDevice.LANDevice.1.WLANConfiguration.{i}.TotalAssociations" "rc" "num"
	if [ $rc -eq 0 ] && [ $num -gt 0 ] && [ $num -le $max_num ]; then
		let uci_num=$num-1
		get_wlan_total_associations $uci_num
	fi
	return $FAULT_CPE_NO_FAULT
	;;	
	InternetGatewayDevice.LANDevice.1.WLANConfiguration.[1-$max_num].WEPKey.1.WEPKey)
	freecwmp_parse_formated_parameter "$1" "InternetGatewayDevice.LANDevice.1.WLANConfiguration.{i}.WEPKey.1.WEPKey" "rc" "num"
	if [ $rc -eq 0 ] && [ $num -gt 0 ] && [ $num -le $max_num ]; then
		let uci_num=$num-1
		get_wlan_wep_key $uci_num "1"
	fi
	return $FAULT_CPE_NO_FAULT
	;;
	InternetGatewayDevice.LANDevice.1.WLANConfiguration.[1-$max_num].WEPKey.2.WEPKey)
	freecwmp_parse_formated_parameter "$1" "InternetGatewayDevice.LANDevice.1.WLANConfiguration.{i}.WEPKey.2.WEPKey" "rc" "num"
	if [ $rc -eq 0 ] && [ $num -gt 0 ] && [ $num -le $max_num ]; then
		let uci_num=$num-1
		get_wlan_wep_key $uci_num "2"
	fi
	return $FAULT_CPE_NO_FAULT
	;;
	InternetGatewayDevice.LANDevice.1.WLANConfiguration.[1-$max_num].WEPKey.3.WEPKey)
	freecwmp_parse_formated_parameter "$1" "InternetGatewayDevice.LANDevice.1.WLANConfiguration.{i}.WEPKey.3.WEPKey" "rc" "num"
	if [ $rc -eq 0 ] && [ $num -gt 0 ] && [ $num -le $max_num ]; then
		let uci_num=$num-1
		get_wlan_wep_key $uci_num "3"
	fi
	return $FAULT_CPE_NO_FAULT
	;;
	InternetGatewayDevice.LANDevice.1.WLANConfiguration.[1-$max_num].WEPKey.4.WEPKey)
	freecwmp_parse_formated_parameter "$1" "InternetGatewayDevice.LANDevice.1.WLANConfiguration.{i}.WEPKey.4.WEPKey" "rc" "num"
	if [ $rc -eq 0 ] && [ $num -gt 0 ] && [ $num -le $max_num ]; then
		let uci_num=$num-1
		get_wlan_wep_key $uci_num "4"
	fi
	return $FAULT_CPE_NO_FAULT
	;;
	InternetGatewayDevice.LANDevice.1.WLANConfiguration.[1-$max_num].PreSharedKey.1.PreSharedKey)
	freecwmp_parse_formated_parameter "$1" "InternetGatewayDevice.LANDevice.1.WLANConfiguration.{i}.PreSharedKey.1.PreSharedKey" "rc" "num"
	if [ $rc -eq 0 ] && [ $num -gt 0 ] && [ $num -le $max_num ]; then
		let uci_num=$num-1
		get_wlan_pre_shared_key $uci_num "1"
	fi
	return $FAULT_CPE_NO_FAULT
	;;
	InternetGatewayDevice.LANDevice.1.WLANConfiguration.[1-$max_num].PreSharedKey.1.KeyPassphrase)
	freecwmp_parse_formated_parameter "$1" "InternetGatewayDevice.LANDevice.1.WLANConfiguration.{i}.PreSharedKey.1.KeyPassphrase" "rc" "num"
	if [ $rc -eq 0 ] && [ $num -gt 0 ] && [ $num -le $max_num ]; then
		let uci_num=$num-1
		get_wlan_pre_shared_key_key_passphrase $uci_num "1"
	fi
	return $FAULT_CPE_NO_FAULT
	;;
	InternetGatewayDevice.LANDevice.1.WLANConfiguration.[1-$max_num].PreSharedKey.1.AssociatedDeviceMACAddress)
	freecwmp_parse_formated_parameter "$1" "InternetGatewayDevice.LANDevice.1.WLANConfiguration.{i}.PreSharedKey.1.AssociatedDeviceMACAddress" "rc" "num"
	if [ $rc -eq 0 ] && [ $num -gt 0 ] && [ $num -le $max_num ]; then
		let uci_num=$num-1
		get_wlan_pre_shared_key_associated_device_MACAddress $uci_num "1"
	fi
	return $FAULT_CPE_NO_FAULT
	;;
esac
return $FAULT_CPE_INVALID_PARAMETER_NAME
}

get_lan_device_name() {
local max_num

get_wlan_number_of_entries "max_num"

case "$1" in
	InternetGatewayDevice.)
	freecwmp_output "InternetGatewayDevice.LANDevice." "" "1"
	if [ "$2" = "0" ]; then
		freecwmp_output "InternetGatewayDevice.LANDevice.1." "" "1"
		freecwmp_output "InternetGatewayDevice.LANDevice.1.WLANConfiguration." "" "1"
		for i in `seq 1 $max_num`;do
			freecwmp_parse_formated_parameter $1"LANDevice.1.WLANConfiguration.$i." "InternetGatewayDevice.LANDevice.1.WLANConfiguration.{i}." "rc" "num"
			if [ $rc -eq 0 ] && [ $num -gt 0 ] && [ $num -le $max_num ]; then
				freecwmp_output "InternetGatewayDevice.LANDevice.1.WLANConfiguration.$i." "" "1"
				let uci_num=$num-1
				get_wlan_enable $uci_num
				get_wlan_status $uci_num
				get_wlan_bssid $uci_num
				get_wlan_max_bit_rate $uci_num
				get_wlan_channel $uci_num
				get_wlan_ssid $uci_num
				get_wlan_beacon_type $uci_num
				get_wlan_mac_control_enable $uci_num
				get_wlan_standard $uci_num
				get_wlan_wep_key_index $uci_num
				get_wlan_key_passphrase $uci_num
				get_wlan_wep_encryption_level $uci_num
				get_wlan_basic_encryption_modes $uci_num
				get_wlan_basic_authentication_mode $uci_num
				get_wlan_wpa_encryption_modes $uci_num
				get_wlan_wpa_authentication_mode $uci_num
				get_wlan_ieee_11i_encryption_modes $uci_num
				get_wlan_ieee_11i_authentication_mode $uci_num
				get_wlan_radio_enabled $uci_num
				get_wlan_device_operation_mode $uci_num
				get_wlan_authentication_service_mode $uci_num
				get_wlan_total_associations $uci_num
				get_wlan_associated_device $uci_num
				freecwmp_output "InternetGatewayDevice.LANDevice.1.WLANConfiguration.$i.WEPKey" "" "0"
				get_wlan_wep_key $uci_num "1"
				get_wlan_wep_key $uci_num "2"
				get_wlan_wep_key $uci_num "3"
				get_wlan_wep_key $uci_num "4"
				get_wlan_pre_shared_key $uci_num "1"
				get_wlan_pre_shared_key_key_passphrase $uci_num "1"
				get_wlan_pre_shared_key_associated_device_MACAddress $uci_num "1"
			fi
		done
	fi
	return $FAULT_CPE_NO_FAULT
	;;
	InternetGatewayDevice.LANDevice.)
	freecwmp_output "InternetGatewayDevice.LANDevice." "" "1"
	freecwmp_output "InternetGatewayDevice.LANDevice.1." "" "1"
	if [ "$2" = "0" ]; then
		freecwmp_output "InternetGatewayDevice.LANDevice.1.WLANConfiguration." "" "1"
		for i in `seq 1 $max_num`;do
			freecwmp_parse_formated_parameter $1"1.WLANConfiguration.$i." "InternetGatewayDevice.LANDevice.1.WLANConfiguration.{i}." "rc" "num"
			if [ $rc -eq 0 ] && [ $num -gt 0 ] && [ $num -le $max_num ]; then
				freecwmp_output "InternetGatewayDevice.LANDevice.1.WLANConfiguration.$i." "" "1"
				let uci_num=$num-1
				get_wlan_enable $uci_num
				get_wlan_status $uci_num
				get_wlan_bssid $uci_num
				get_wlan_max_bit_rate $uci_num
				get_wlan_channel $uci_num
				get_wlan_ssid $uci_num
				get_wlan_beacon_type $uci_num
				get_wlan_mac_control_enable $uci_num
				get_wlan_standard $uci_num
				get_wlan_wep_key_index $uci_num
				get_wlan_key_passphrase $uci_num
				get_wlan_wep_encryption_level $uci_num
				get_wlan_basic_encryption_modes $uci_num
				get_wlan_basic_authentication_mode $uci_num
				get_wlan_wpa_encryption_modes $uci_num
				get_wlan_wpa_authentication_mode $uci_num
				get_wlan_ieee_11i_encryption_modes $uci_num
				get_wlan_ieee_11i_authentication_mode $uci_num
				get_wlan_radio_enabled $uci_num
				get_wlan_device_operation_mode $uci_num
				get_wlan_authentication_service_mode $uci_num
				get_wlan_total_associations $uci_num
				get_wlan_associated_device $uci_num
				freecwmp_output "InternetGatewayDevice.LANDevice.1.WLANConfiguration.$i.WEPKey" "" "0"
				get_wlan_wep_key $uci_num "1"
				get_wlan_wep_key $uci_num "2"
				get_wlan_wep_key $uci_num "3"
				get_wlan_wep_key $uci_num "4"
				get_wlan_pre_shared_key $uci_num "1"
				get_wlan_pre_shared_key_key_passphrase $uci_num "1"
				get_wlan_pre_shared_key_associated_device_MACAddress $uci_num "1"
			fi
		done
	fi
	return $FAULT_CPE_NO_FAULT
	;;
	InternetGatewayDevice.LANDevice.1.)
	freecwmp_output "InternetGatewayDevice.LANDevice.1." "" "1"
	freecwmp_output "InternetGatewayDevice.LANDevice.1.WLANConfiguration." "" "1"
	if [ "$2" = "0" ]; then
		for i in `seq 1 $max_num`;do
			freecwmp_parse_formated_parameter $1"WLANConfiguration.$i." "InternetGatewayDevice.LANDevice.1.WLANConfiguration.{i}." "rc" "num"
			if [ $rc -eq 0 ] && [ $num -gt 0 ] && [ $num -le $max_num ]; then
				freecwmp_output "InternetGatewayDevice.LANDevice.1.WLANConfiguration.$i." "" "1"
				let uci_num=$num-1
				get_wlan_enable $uci_num
				get_wlan_status $uci_num
				get_wlan_bssid $uci_num
				get_wlan_max_bit_rate $uci_num
				get_wlan_channel $uci_num
				get_wlan_ssid $uci_num
				get_wlan_beacon_type $uci_num
				get_wlan_mac_control_enable $uci_num
				get_wlan_standard $uci_num
				get_wlan_wep_key_index $uci_num
				get_wlan_key_passphrase $uci_num
				get_wlan_wep_encryption_level $uci_num
				get_wlan_basic_encryption_modes $uci_num
				get_wlan_basic_authentication_mode $uci_num
				get_wlan_wpa_encryption_modes $uci_num
				get_wlan_wpa_authentication_mode $uci_num
				get_wlan_ieee_11i_encryption_modes $uci_num
				get_wlan_ieee_11i_authentication_mode $uci_num
				get_wlan_radio_enabled $uci_num
				get_wlan_device_operation_mode $uci_num
				get_wlan_authentication_service_mode $uci_num
				get_wlan_total_associations $uci_num
				get_wlan_associated_device $uci_num
				freecwmp_output "InternetGatewayDevice.LANDevice.1.WLANConfiguration.$i.WEPKey" "" "0"
				get_wlan_wep_key $uci_num "1"
				get_wlan_wep_key $uci_num "2"
				get_wlan_wep_key $uci_num "3"
				get_wlan_wep_key $uci_num "4"
				get_wlan_pre_shared_key $uci_num "1"
				get_wlan_pre_shared_key_key_passphrase $uci_num "1"
				get_wlan_pre_shared_key_associated_device_MACAddress $uci_num "1"
			fi
		done
	fi
	return $FAULT_CPE_NO_FAULT
	;;
	InternetGatewayDevice.LANDevice.1.WLANConfiguration.)
	freecwmp_output "InternetGatewayDevice.LANDevice.1.WLANConfiguration." "" "1"
	if [ "$2" = "0" ]; then
		for i in `seq 1 $max_num`;do
			freecwmp_parse_formated_parameter "$1$i." "InternetGatewayDevice.LANDevice.1.WLANConfiguration.{i}." "rc" "num"
			if [ $rc -eq 0 ] && [ $num -gt 0 ] && [ $num -le $max_num ]; then
				freecwmp_output "InternetGatewayDevice.LANDevice.1.WLANConfiguration.$i." "" "1"
				let uci_num=$num-1
				get_wlan_enable $uci_num
				get_wlan_status $uci_num
				get_wlan_bssid $uci_num
				get_wlan_max_bit_rate $uci_num
				get_wlan_channel $uci_num
				get_wlan_ssid $uci_num
				get_wlan_beacon_type $uci_num
				get_wlan_mac_control_enable $uci_num
				get_wlan_standard $uci_num
				get_wlan_wep_key_index $uci_num
				get_wlan_key_passphrase $uci_num
				get_wlan_wep_encryption_level $uci_num
				get_wlan_basic_encryption_modes $uci_num
				get_wlan_basic_authentication_mode $uci_num
				get_wlan_wpa_encryption_modes $uci_num
				get_wlan_wpa_authentication_mode $uci_num
				get_wlan_ieee_11i_encryption_modes $uci_num
				get_wlan_ieee_11i_authentication_mode $uci_num
				get_wlan_radio_enabled $uci_num
				get_wlan_device_operation_mode $uci_num
				get_wlan_authentication_service_mode $uci_num
				get_wlan_total_associations $uci_num
				get_wlan_associated_device $uci_num
				freecwmp_output "InternetGatewayDevice.LANDevice.1.WLANConfiguration.$i.WEPKey" "" "0"
				get_wlan_wep_key $uci_num "1"
				get_wlan_wep_key $uci_num "2"
				get_wlan_wep_key $uci_num "3"
				get_wlan_wep_key $uci_num "4"
				get_wlan_pre_shared_key $uci_num "1"
				get_wlan_pre_shared_key_key_passphrase $uci_num "1"
				get_wlan_pre_shared_key_associated_device_MACAddress $uci_num "1"
			fi
		done
	fi
	return $FAULT_CPE_NO_FAULT
	;;
	InternetGatewayDevice.LANDevice.1.WLANConfiguration.[1-$max_num].)
	freecwmp_output "$1" "" "1"
	if [ "$2" = "0" ]; then
		freecwmp_parse_formated_parameter "$1" "InternetGatewayDevice.LANDevice.1.WLANConfiguration.{i}." "rc" "num"
		if [ $rc -eq 0 ] && [ $num -gt 0 ] && [ $num -le $max_num ]; then
			let uci_num=$num-1
			get_wlan_enable $uci_num
			get_wlan_status $uci_num
			get_wlan_bssid $uci_num
			get_wlan_max_bit_rate $uci_num
			get_wlan_channel $uci_num
			get_wlan_ssid $uci_num
			get_wlan_beacon_type $uci_num
			get_wlan_mac_control_enable $uci_num
			get_wlan_standard $uci_num
			get_wlan_wep_key_index $uci_num
			get_wlan_key_passphrase $uci_num
			get_wlan_wep_encryption_level $uci_num
			get_wlan_basic_encryption_modes $uci_num
			get_wlan_basic_authentication_mode $uci_num
			get_wlan_wpa_encryption_modes $uci_num
			get_wlan_wpa_authentication_mode $uci_num
			get_wlan_ieee_11i_encryption_modes $uci_num
			get_wlan_ieee_11i_authentication_mode $uci_num
			get_wlan_radio_enabled $uci_num
			get_wlan_device_operation_mode $uci_num
			get_wlan_authentication_service_mode $uci_num
			get_wlan_total_associations $uci_num
			get_wlan_associated_device $uci_num
			freecwmp_output "InternetGatewayDevice.LANDevice.1.WLANConfiguration.$i.WEPKey" "" "0"
			get_wlan_wep_key $uci_num "1"
			get_wlan_wep_key $uci_num "2"
			get_wlan_wep_key $uci_num "3"
			get_wlan_wep_key $uci_num "4"
			get_wlan_pre_shared_key $uci_num "1"
			get_wlan_pre_shared_key_key_passphrase $uci_num "1"
			get_wlan_pre_shared_key_associated_device_MACAddress $uci_num "1"
		fi
	fi
	return $FAULT_CPE_NO_FAULT
	;;
	InternetGatewayDevice.LANDevice.1.WLANConfiguration.[1-$max_num].Enable)
	if [ "$2" = "1" ]; then
		return $FAULT_CPE_INVALID_ARGUMENTS
	fi
	freecwmp_parse_formated_parameter "$1" "InternetGatewayDevice.LANDevice.1.WLANConfiguration.{i}.Enable" "rc" "num"
	if [ $rc -eq 0 ] && [ $num -gt 0 ] && [ $num -le $max_num ]; then
		let uci_num=$num-1
		get_wlan_enable $uci_num
	fi
	return $FAULT_CPE_NO_FAULT
	;;
	InternetGatewayDevice.LANDevice.1.WLANConfiguration.[1-$max_num].Status)
	if [ "$2" = "1" ]; then
		return $FAULT_CPE_INVALID_ARGUMENTS
	fi
	freecwmp_parse_formated_parameter "$1" "InternetGatewayDevice.LANDevice.1.WLANConfiguration.{i}.Status" "rc" "num"
	if [ $rc -eq 0 ] && [ $num -gt 0 ] && [ $num -le $max_num ]; then
		let uci_num=$num-1
		get_wlan_status $uci_num
	fi
	return $FAULT_CPE_NO_FAULT
	;;
	InternetGatewayDevice.LANDevice.1.WLANConfiguration.[1-$max_num].BSSID)
	if [ "$2" = "1" ]; then
		return $FAULT_CPE_INVALID_ARGUMENTS
	fi
	freecwmp_parse_formated_parameter "$1" "InternetGatewayDevice.LANDevice.1.WLANConfiguration.{i}.BSSID" "rc" "num"
	if [ $rc -eq 0 ] && [ $num -gt 0 ] && [ $num -le $max_num ]; then
		let uci_num=$num-1
		get_wlan_bssid $uci_num
	fi
	return $FAULT_CPE_NO_FAULT
	;;
	InternetGatewayDevice.LANDevice.1.WLANConfiguration.[1-$max_num].MaxBitRate)
	if [ "$2" = "1" ]; then
		return $FAULT_CPE_INVALID_ARGUMENTS
	fi
	freecwmp_parse_formated_parameter "$1" "InternetGatewayDevice.LANDevice.1.WLANConfiguration.{i}.MaxBitRate" "rc" "num"
	if [ $rc -eq 0 ] && [ $num -gt 0 ] && [ $num -le $max_num ]; then
		let uci_num=$num-1
		get_wlan_max_bit_rate $uci_num
	fi
	return $FAULT_CPE_NO_FAULT
	;;
	InternetGatewayDevice.LANDevice.1.WLANConfiguration.[1-$max_num].Channel)
	if [ "$2" = "1" ]; then
		return $FAULT_CPE_INVALID_ARGUMENTS
	fi
	freecwmp_parse_formated_parameter "$1" "InternetGatewayDevice.LANDevice.1.WLANConfiguration.{i}.Channel" "rc" "num"
	if [ $rc -eq 0 ] && [ $num -gt 0 ] && [ $num -le $max_num ]; then
		let uci_num=$num-1
		get_wlan_channel $uci_num
	fi
	return $FAULT_CPE_NO_FAULT
	;;
	InternetGatewayDevice.LANDevice.1.WLANConfiguration.[1-$max_num].SSID)
	if [ "$2" = "1" ]; then
		return $FAULT_CPE_INVALID_ARGUMENTS
	fi
	freecwmp_parse_formated_parameter "$1" "InternetGatewayDevice.LANDevice.1.WLANConfiguration.{i}.SSID" "rc" "num"
	if [ $rc -eq 0 ] && [ $num -gt 0 ] && [ $num -le $max_num ]; then
		let uci_num=$num-1
		get_wlan_ssid $uci_num
	fi
	return $FAULT_CPE_NO_FAULT
	;;
	InternetGatewayDevice.LANDevice.1.WLANConfiguration.[1-$max_num].BeaconType)
	if [ "$2" = "1" ]; then
		return $FAULT_CPE_INVALID_ARGUMENTS
	fi
	freecwmp_parse_formated_parameter "$1" "InternetGatewayDevice.LANDevice.1.WLANConfiguration.{i}.BeaconType" "rc" "num"
	if [ $rc -eq 0 ] && [ $num -gt 0 ] && [ $num -le $max_num ]; then
		let uci_num=$num-1
		get_wlan_beacon_type $uci_num
	fi
	return $FAULT_CPE_NO_FAULT
	;;
	InternetGatewayDevice.LANDevice.1.WLANConfiguration.[1-$max_num].MACAddressControlEnabled)
	if [ "$2" = "1" ]; then
		return $FAULT_CPE_INVALID_ARGUMENTS
	fi
	freecwmp_parse_formated_parameter "$1" "InternetGatewayDevice.LANDevice.1.WLANConfiguration.{i}.MACAddressControlEnabled" "rc" "num"
	if [ $rc -eq 0 ] && [ $num -gt 0 ] && [ $num -le $max_num ]; then
		let uci_num=$num-1
		get_wlan_mac_control_enable $uci_num
	fi
	return $FAULT_CPE_NO_FAULT
	;;
	InternetGatewayDevice.LANDevice.1.WLANConfiguration.[1-$max_num].Standard)
	if [ "$2" = "1" ]; then
		return $FAULT_CPE_INVALID_ARGUMENTS
	fi
	freecwmp_parse_formated_parameter "$1" "InternetGatewayDevice.LANDevice.1.WLANConfiguration.{i}.Standard" "rc" "num"
	if [ $rc -eq 0 ] && [ $num -gt 0 ] && [ $num -le $max_num ]; then
		let uci_num=$num-1
		get_wlan_standard $uci_num
	fi
	return $FAULT_CPE_NO_FAULT
	;;
	InternetGatewayDevice.LANDevice.1.WLANConfiguration.[1-$max_num].WEPKeyIndex)
	if [ "$2" = "1" ]; then
		return $FAULT_CPE_INVALID_ARGUMENTS
	fi
	freecwmp_parse_formated_parameter "$1" "InternetGatewayDevice.LANDevice.1.WLANConfiguration.{i}.WEPKeyIndex" "rc" "num"
	if [ $rc -eq 0 ] && [ $num -gt 0 ] && [ $num -le $max_num ]; then
		let uci_num=$num-1
		get_wlan_wep_key_index $uci_num
	fi
	return $FAULT_CPE_NO_FAULT
	;;
	InternetGatewayDevice.LANDevice.1.WLANConfiguration.[1-$max_num].KeyPassphrase)
	if [ "$2" = "1" ]; then
		return $FAULT_CPE_INVALID_ARGUMENTS
	fi
	freecwmp_parse_formated_parameter "$1" "InternetGatewayDevice.LANDevice.1.WLANConfiguration.{i}.KeyPassphrase" "rc" "num"
	if [ $rc -eq 0 ] && [ $num -gt 0 ] && [ $num -le $max_num ]; then
		let uci_num=$num-1
		get_wlan_key_passphrase $uci_num
	fi
	return $FAULT_CPE_NO_FAULT
	;;
	InternetGatewayDevice.LANDevice.1.WLANConfiguration.[1-$max_num].WEPEncryptionLevel)
	freecwmp_parse_formated_parameter "$1" "InternetGatewayDevice.LANDevice.1.WLANConfiguration.{i}.WEPEncryptionLevel" "rc" "num"
	if [ $rc -eq 0 ] && [ $num -gt 0 ] && [ $num -le $max_num ]; then
		let uci_num=$num-1
		get_wlan_wep_encryption_level $uci_num
	fi
	return $FAULT_CPE_NO_FAULT
	;;
	InternetGatewayDevice.LANDevice.1.WLANConfiguration.[1-$max_num].BasicEncryptionModes)
	if [ "$2" = "1" ]; then
		return $FAULT_CPE_INVALID_ARGUMENTS
	fi
	freecwmp_parse_formated_parameter "$1" "InternetGatewayDevice.LANDevice.1.WLANConfiguration.{i}.BasicEncryptionModes" "rc" "num"
	if [ $rc -eq 0 ] && [ $num -gt 0 ] && [ $num -le $max_num ]; then
		let uci_num=$num-1
		get_wlan_basic_encryption_modes $uci_num
	fi
	return $FAULT_CPE_NO_FAULT
	;;
	InternetGatewayDevice.LANDevice.1.WLANConfiguration.[1-$max_num].BasicAuthenticationMode)
	if [ "$2" = "1" ]; then
		return $FAULT_CPE_INVALID_ARGUMENTS
	fi
	freecwmp_parse_formated_parameter "$1" "InternetGatewayDevice.LANDevice.1.WLANConfiguration.{i}.BasicAuthenticationMode" "rc" "num"
	if [ $rc -eq 0 ] && [ $num -gt 0 ] && [ $num -le $max_num ]; then
		let uci_num=$num-1
		get_wlan_basic_authentication_mode $uci_num
	fi
	return $FAULT_CPE_NO_FAULT
	;;
	InternetGatewayDevice.LANDevice.1.WLANConfiguration.[1-$max_num].WPAEncryptionModes)
	if [ "$2" = "1" ]; then
		return $FAULT_CPE_INVALID_ARGUMENTS
	fi
	freecwmp_parse_formated_parameter "$1" "InternetGatewayDevice.LANDevice.1.WLANConfiguration.{i}.WPAEncryptionModes" "rc" "num"
	if [ $rc -eq 0 ] && [ $num -gt 0 ] && [ $num -le $max_num ]; then
		let uci_num=$num-1
		get_wlan_wpa_encryption_modes $uci_num
	fi
	return $FAULT_CPE_NO_FAULT
	;;
	InternetGatewayDevice.LANDevice.1.WLANConfiguration.[1-$max_num].WPAAuthenticationMode)
	if [ "$2" = "1" ]; then
		return $FAULT_CPE_INVALID_ARGUMENTS
	fi
	freecwmp_parse_formated_parameter "$1" "InternetGatewayDevice.LANDevice.1.WLANConfiguration.{i}.WPAAuthenticationMode" "rc" "num"
	if [ $rc -eq 0 ] && [ $num -gt 0 ] && [ $num -le $max_num ]; then
		let uci_num=$num-1
		get_wlan_wpa_authentication_mode $uci_num
	fi
	return $FAULT_CPE_NO_FAULT
	;;
	InternetGatewayDevice.LANDevice.1.WLANConfiguration.[1-$max_num].IEEE11iEncryptionModes)
	if [ "$2" = "1" ]; then
		return $FAULT_CPE_INVALID_ARGUMENTS
	fi
	freecwmp_parse_formated_parameter "$1" "InternetGatewayDevice.LANDevice.1.WLANConfiguration.{i}.IEEE11iEncryptionModes" "rc" "num"
	if [ $rc -eq 0 ] && [ $num -gt 0 ] && [ $num -le $max_num ]; then
		let uci_num=$num-1
		get_wlan_ieee_11i_encryption_modes $uci_num
	fi
	return $FAULT_CPE_NO_FAULT
	;;
	InternetGatewayDevice.LANDevice.1.WLANConfiguration.[1-$max_num].IEEE11iAuthenticationMode)
	if [ "$2" = "1" ]; then
		return $FAULT_CPE_INVALID_ARGUMENTS
	fi
	freecwmp_parse_formated_parameter "$1" "InternetGatewayDevice.LANDevice.1.WLANConfiguration.{i}.IEEE11iAuthenticationMode" "rc" "num"
	if [ $rc -eq 0 ] && [ $num -gt 0 ] && [ $num -le $max_num ]; then
		let uci_num=$num-1
		get_wlan_ieee_11i_authentication_mode $uci_num
	fi
	return $FAULT_CPE_NO_FAULT
	;;
	InternetGatewayDevice.LANDevice.1.WLANConfiguration.[1-$max_num].RadioEnabled)
	if [ "$2" = "1" ]; then
		return $FAULT_CPE_INVALID_ARGUMENTS
	fi
	freecwmp_parse_formated_parameter "$1" "InternetGatewayDevice.LANDevice.1.WLANConfiguration.{i}.RadioEnabled" "rc" "num"
	if [ $rc -eq 0 ] && [ $num -gt 0 ] && [ $num -le $max_num ]; then
		let uci_num=$num-1
		get_wlan_radio_enabled $uci_num
	fi
	return $FAULT_CPE_NO_FAULT
	;;
	InternetGatewayDevice.LANDevice.1.WLANConfiguration.[1-$max_num].DeviceOperationMode)
	if [ "$2" = "1" ]; then
		return $FAULT_CPE_INVALID_ARGUMENTS
	fi
	freecwmp_parse_formated_parameter "$1" "InternetGatewayDevice.LANDevice.1.WLANConfiguration.{i}.DeviceOperationMode" "rc" "num"
	if [ $rc -eq 0 ] && [ $num -gt 0 ] && [ $num -le $max_num ]; then
		let uci_num=$num-1
		get_wlan_device_operation_mode $uci_num
	fi
	return $FAULT_CPE_NO_FAULT
	;;
	InternetGatewayDevice.LANDevice.1.WLANConfiguration.[1-$max_num].AuthenticationServiceMode)
	if [ "$2" = "1" ]; then
		return $FAULT_CPE_INVALID_ARGUMENTS
	fi
	freecwmp_parse_formated_parameter "$1" "InternetGatewayDevice.LANDevice.1.WLANConfiguration.{i}.AuthenticationServiceMode" "rc" "num"
	if [ $rc -eq 0 ] && [ $num -gt 0 ] && [ $num -le $max_num ]; then
		let uci_num=$num-1
		get_wlan_authentication_service_mode $uci_num
	fi
	return $FAULT_CPE_NO_FAULT
	;;
	InternetGatewayDevice.LANDevice.1.WLANConfiguration.[1-$max_num].TotalAssociations)
	if [ "$2" = "1" ]; then
		return $FAULT_CPE_INVALID_ARGUMENTS
	fi
	freecwmp_parse_formated_parameter "$1" "InternetGatewayDevice.LANDevice.1.WLANConfiguration.{i}.TotalAssociations" "rc" "num"
	if [ $rc -eq 0 ] && [ $num -gt 0 ] && [ $num -le $max_num ]; then
		let uci_num=$num-1
		get_wlan_total_associations $uci_num
	fi
	return $FAULT_CPE_NO_FAULT
	;;
	InternetGatewayDevice.LANDevice.1.WLANConfiguration.[1-$max_num].WEPKey.1.WEPKey)
	if [ "$2" = "1" ]; then
		return $FAULT_CPE_INVALID_ARGUMENTS
	fi
	freecwmp_parse_formated_parameter "$1" "InternetGatewayDevice.LANDevice.1.WLANConfiguration.{i}.WEPKey.1.WEPKey" "rc" "num"
	if [ $rc -eq 0 ] && [ $num -gt 0 ] && [ $num -le $max_num ]; then
		let uci_num=$num-1
		get_wlan_wep_key $uci_num "1"
	fi
	return $FAULT_CPE_NO_FAULT
	;;
	InternetGatewayDevice.LANDevice.1.WLANConfiguration.[1-$max_num].WEPKey.2.WEPKey)
	if [ "$2" = "1" ]; then
		return $FAULT_CPE_INVALID_ARGUMENTS
	fi
	freecwmp_parse_formated_parameter "$1" "InternetGatewayDevice.LANDevice.1.WLANConfiguration.{i}.WEPKey.2.WEPKey" "rc" "num"
	if [ $rc -eq 0 ] && [ $num -gt 0 ] && [ $num -le $max_num ]; then
		let uci_num=$num-1
		get_wlan_wep_key $uci_num "2"
	fi
	return $FAULT_CPE_NO_FAULT
	;;
	InternetGatewayDevice.LANDevice.1.WLANConfiguration.[1-$max_num].WEPKey.3.WEPKey)
	if [ "$2" = "1" ]; then
		return $FAULT_CPE_INVALID_ARGUMENTS
	fi
	freecwmp_parse_formated_parameter "$1" "InternetGatewayDevice.LANDevice.1.WLANConfiguration.{i}.WEPKey.3.WEPKey" "rc" "num"
	if [ $rc -eq 0 ] && [ $num -gt 0 ] && [ $num -le $max_num ]; then
		let uci_num=$num-1
		get_wlan_wep_key $uci_num "3"
	fi
	return $FAULT_CPE_NO_FAULT
	;;
	InternetGatewayDevice.LANDevice.1.WLANConfiguration.[1-$max_num].WEPKey.4.WEPKey)
	if [ "$2" = "1" ]; then
		return $FAULT_CPE_INVALID_ARGUMENTS
	fi
	freecwmp_parse_formated_parameter "$1" "InternetGatewayDevice.LANDevice.1.WLANConfiguration.{i}.WEPKey.4.WEPKey" "rc" "num"
	if [ $rc -eq 0 ] && [ $num -gt 0 ] && [ $num -le $max_num ]; then
		let uci_num=$num-1
		get_wlan_wep_key $uci_num "4"
	fi
	return $FAULT_CPE_NO_FAULT
	;;
		InternetGatewayDevice.LANDevice.1.WLANConfiguration.[1-$max_num].PreSharedKey.1.WEPKey)
	if [ "$2" = "1" ]; then
		return $FAULT_CPE_INVALID_ARGUMENTS
	fi
	freecwmp_parse_formated_parameter "$1" "InternetGatewayDevice.LANDevice.1.WLANConfiguration.{i}.PreSharedKey.1.WEPKey" "rc" "num"
	if [ $rc -eq 0 ] && [ $num -gt 0 ] && [ $num -le $max_num ]; then
		let uci_num=$num-1
		get_wlan_pre_shared_key $uci_num "1"
	fi
	return $FAULT_CPE_NO_FAULT
	;;
		InternetGatewayDevice.LANDevice.1.WLANConfiguration.[1-$max_num].PreSharedKey.1.KeyPassphrase)
	if [ "$2" = "1" ]; then
		return $FAULT_CPE_INVALID_ARGUMENTS
	fi
	freecwmp_parse_formated_parameter "$1" "InternetGatewayDevice.LANDevice.1.WLANConfiguration.{i}.PreSharedKey.1.KeyPassphrase" "rc" "num"
	if [ $rc -eq 0 ] && [ $num -gt 0 ] && [ $num -le $max_num ]; then
		let uci_num=$num-1
		get_wlan_pre_shared_key_key_passphrase $uci_num "1"
	fi
	return $FAULT_CPE_NO_FAULT
	;;
		InternetGatewayDevice.LANDevice.1.WLANConfiguration.[1-$max_num].PreSharedKey.1.AssociatedDeviceMACAddress)
	if [ "$2" = "1" ]; then
		return $FAULT_CPE_INVALID_ARGUMENTS
	fi
	freecwmp_parse_formated_parameter "$1" "InternetGatewayDevice.LANDevice.1.WLANConfiguration.{i}.PreSharedKey.1.AssociatedDeviceMACAddress" "rc" "num"
	if [ $rc -eq 0 ] && [ $num -gt 0 ] && [ $num -le $max_num ]; then
		let uci_num=$num-1
		get_wlan_pre_shared_key_associated_device_MACAddress $uci_num "1"
	fi
	return $FAULT_CPE_NO_FAULT
	;;
esac
return $FAULT_CPE_INVALID_PARAMETER_NAME
}

get_lan_device_notification() {
local max_num

get_wlan_number_of_entries "max_num"

case "$1" in
	InternetGatewayDevice.)
	get_ip_interface_ip_address
	get_ip_interface_netmask
	for i in `seq 1 $max_num`;do
		freecwmp_parse_formated_parameter $1"LANDevice.1.WLANConfiguration.$i." "InternetGatewayDevice.LANDevice.1.WLANConfiguration.{i}." "rc" "num"
		if [ $rc -eq 0 ] && [ $num -gt 0 ] && [ $num -le $max_num ]; then
			let uci_num=$num-1
			get_wlan_enable $uci_num
			get_wlan_status $uci_num
			get_wlan_bssid $uci_num
			get_wlan_max_bit_rate $uci_num
			get_wlan_channel $uci_num
			get_wlan_ssid $uci_num
			get_wlan_beacon_type $uci_num
			get_wlan_mac_control_enable $uci_num
			get_wlan_standard $uci_num
			get_wlan_wep_key_index $uci_num
			get_wlan_key_passphrase $uci_num
			get_wlan_wep_encryption_level $uci_num
			get_wlan_basic_encryption_modes $uci_num
			get_wlan_basic_authentication_mode $uci_num
			get_wlan_wpa_encryption_modes $uci_num
			get_wlan_wpa_authentication_mode $uci_num
			get_wlan_ieee_11i_encryption_modes $uci_num
			get_wlan_ieee_11i_authentication_mode $uci_num
			get_wlan_radio_enabled $uci_num
			get_wlan_device_operation_mode $uci_num
			get_wlan_authentication_service_mode $uci_num
			get_wlan_total_associations $uci_num
			get_wlan_associated_device $uci_num
			get_wlan_wep_key $uci_num "1"
			get_wlan_wep_key $uci_num "2"
			get_wlan_wep_key $uci_num "3"
			get_wlan_wep_key $uci_num "4"
			get_wlan_pre_shared_key $uci_num "1"
			get_wlan_pre_shared_key_key_passphrase $uci_num "1"
			get_wlan_pre_shared_key_associated_device_MACAddress $uci_num "1"
		fi
	done
	return $FAULT_CPE_NO_FAULT
	;;
	InternetGatewayDevice.LANDevice.)
	get_ip_interface_ip_address
	get_ip_interface_netmask
	for i in `seq 1 $max_num`;do
		freecwmp_parse_formated_parameter $1"1.WLANConfiguration.$i." "InternetGatewayDevice.LANDevice.1.WLANConfiguration.{i}." "rc" "num"
		if [ $rc -eq 0 ] && [ $num -gt 0 ] && [ $num -le $max_num ]; then
			let uci_num=$num-1
			get_wlan_enable $uci_num
			get_wlan_status $uci_num
			get_wlan_bssid $uci_num
			get_wlan_max_bit_rate $uci_num
			get_wlan_channel $uci_num
			get_wlan_ssid $uci_num
			get_wlan_beacon_type $uci_num
			get_wlan_mac_control_enable $uci_num
			get_wlan_standard $uci_num
			get_wlan_wep_key_index $uci_num
			get_wlan_key_passphrase $uci_num
			get_wlan_wep_encryption_level $uci_num
			get_wlan_basic_encryption_modes $uci_num
			get_wlan_basic_authentication_mode $uci_num
			get_wlan_wpa_encryption_modes $uci_num
			get_wlan_wpa_authentication_mode $uci_num
			get_wlan_ieee_11i_encryption_modes $uci_num
			get_wlan_ieee_11i_authentication_mode $uci_num
			get_wlan_radio_enabled $uci_num
			get_wlan_device_operation_mode $uci_num
			get_wlan_authentication_service_mode $uci_num
			get_wlan_total_associations $uci_num
			get_wlan_associated_device $uci_num
			get_wlan_wep_key $uci_num "1"
			get_wlan_wep_key $uci_num "2"
			get_wlan_wep_key $uci_num "3"
			get_wlan_wep_key $uci_num "4"
			get_wlan_pre_shared_key $uci_num "1"
			get_wlan_pre_shared_key_key_passphrase $uci_num "1"
			get_wlan_pre_shared_key_associated_device_MACAddress $uci_num "1"
		fi
	done
	return $FAULT_CPE_NO_FAULT
	;;
	InternetGatewayDevice.LANDevice.1.)
	get_ip_interface_ip_address
	get_ip_interface_netmask
	for i in `seq 1 $max_num`;do
		freecwmp_parse_formated_parameter $1"WLANConfiguration.$i." "InternetGatewayDevice.LANDevice.1.WLANConfiguration.{i}." "rc" "num"
		if [ $rc -eq 0 ] && [ $num -gt 0 ] && [ $num -le $max_num ]; then
			let uci_num=$num-1
			get_wlan_enable $uci_num
			get_wlan_status $uci_num
			get_wlan_bssid $uci_num
			get_wlan_max_bit_rate $uci_num
			get_wlan_channel $uci_num
			get_wlan_ssid $uci_num
			get_wlan_beacon_type $uci_num
			get_wlan_mac_control_enable $uci_num
			get_wlan_standard $uci_num
			get_wlan_wep_key_index $uci_num
			get_wlan_key_passphrase $uci_num
			get_wlan_wep_encryption_level $uci_num
			get_wlan_basic_encryption_modes $uci_num
			get_wlan_basic_authentication_mode $uci_num
			get_wlan_wpa_encryption_modes $uci_num
			get_wlan_wpa_authentication_mode $uci_num
			get_wlan_ieee_11i_encryption_modes $uci_num
			get_wlan_ieee_11i_authentication_mode $uci_num
			get_wlan_radio_enabled $uci_num
			get_wlan_device_operation_mode $uci_num
			get_wlan_authentication_service_mode $uci_num
			get_wlan_total_associations $uci_num
			get_wlan_associated_device $uci_num
			get_wlan_wep_key $uci_num "1"
			get_wlan_wep_key $uci_num "2"
			get_wlan_wep_key $uci_num "3"
			get_wlan_wep_key $uci_num "4"
			get_wlan_pre_shared_key $uci_num "1"
			get_wlan_pre_shared_key_key_passphrase $uci_num "1"
			get_wlan_pre_shared_key_associated_device_MACAddress $uci_num "1"
		fi
	done
	return $FAULT_CPE_NO_FAULT
	;;
	InternetGatewayDevice.LANDevice.1.LANHostConfigManagement.)
	get_ip_interface_ip_address
	get_ip_interface_netmask
	return $FAULT_CPE_NO_FAULT
	;;
	InternetGatewayDevice.LANDevice.1.LANHostConfigManagement.IPInterface.)
	get_ip_interface_ip_address
	get_ip_interface_netmask
	return $FAULT_CPE_NO_FAULT
	;;
	InternetGatewayDevice.LANDevice.1.LANHostConfigManagement.IPInterface.[1-9]+.)
	get_ip_interface_ip_address
	get_ip_interface_netmask
	return $FAULT_CPE_NO_FAULT
	;;
	InternetGatewayDevice.LANDevice.1.WLANConfiguration.)
	for i in `seq 1 $max_num`;do
		freecwmp_parse_formated_parameter "$1$i." "InternetGatewayDevice.LANDevice.1.WLANConfiguration.{i}." "rc" "num"
		if [ $rc -eq 0 ] && [ $num -gt 0 ] && [ $num -le $max_num ]; then
			let uci_num=$num-1
			get_wlan_enable $uci_num
			get_wlan_status $uci_num
			get_wlan_bssid $uci_num
			get_wlan_max_bit_rate $uci_num
			get_wlan_channel $uci_num
			get_wlan_ssid $uci_num
			get_wlan_beacon_type $uci_num
			get_wlan_mac_control_enable $uci_num
			get_wlan_standard $uci_num
			get_wlan_wep_key_index $uci_num
			get_wlan_key_passphrase $uci_num
			get_wlan_wep_encryption_level $uci_num
			get_wlan_basic_encryption_modes $uci_num
			get_wlan_basic_authentication_mode $uci_num
			get_wlan_wpa_encryption_modes $uci_num
			get_wlan_wpa_authentication_mode $uci_num
			get_wlan_ieee_11i_encryption_modes $uci_num
			get_wlan_ieee_11i_authentication_mode $uci_num
			get_wlan_radio_enabled $uci_num
			get_wlan_device_operation_mode $uci_num
			get_wlan_authentication_service_mode $uci_num
			get_wlan_total_associations $uci_num
			get_wlan_associated_device $uci_num
			get_wlan_wep_key $uci_num "1"
			get_wlan_wep_key $uci_num "2"
			get_wlan_wep_key $uci_num "3"
			get_wlan_wep_key $uci_num "4"
			get_wlan_pre_shared_key $uci_num "1"
			get_wlan_pre_shared_key_key_passphrase $uci_num "1"
			get_wlan_pre_shared_key_associated_device_MACAddress $uci_num "1"
		fi
	done
	return $FAULT_CPE_NO_FAULT
	;;
	InternetGatewayDevice.LANDevice.1.WLANConfiguration.[1-$max_num].)
	freecwmp_parse_formated_parameter "$1" "InternetGatewayDevice.LANDevice.1.WLANConfiguration.{i}." "rc" "num"
	if [ $rc -eq 0 ] && [ $num -gt 0 ] && [ $num -le $max_num ]; then
		let uci_num=$num-1
		get_wlan_enable $uci_num
		get_wlan_status $uci_num
		get_wlan_bssid $uci_num
		get_wlan_max_bit_rate $uci_num
		get_wlan_channel $uci_num
		get_wlan_ssid $uci_num
		get_wlan_beacon_type $uci_num
		get_wlan_mac_control_enable $uci_num
		get_wlan_standard $uci_num
		get_wlan_wep_key_index $uci_num
		get_wlan_key_passphrase $uci_num
		get_wlan_wep_encryption_level $uci_num
		get_wlan_basic_encryption_modes $uci_num
		get_wlan_basic_authentication_mode $uci_num
		get_wlan_wpa_encryption_modes $uci_num
		get_wlan_wpa_authentication_mode $uci_num
		get_wlan_ieee_11i_encryption_modes $uci_num
		get_wlan_ieee_11i_authentication_mode $uci_num
		get_wlan_radio_enabled $uci_num
		get_wlan_device_operation_mode $uci_num
		get_wlan_authentication_service_mode $uci_num
		get_wlan_total_associations $uci_num
		get_wlan_associated_device $uci_num
		get_wlan_wep_key $uci_num "1"
		get_wlan_wep_key $uci_num "2"
		get_wlan_wep_key $uci_num "3"
		get_wlan_wep_key $uci_num "4"
		get_wlan_pre_shared_key $uci_num "1"
		get_wlan_pre_shared_key_key_passphrase $uci_num "1"
		get_wlan_pre_shared_key_associated_device_MACAddress $uci_num "1"
	fi
	return $FAULT_CPE_NO_FAULT
	;;
	InternetGatewayDevice.LANDevice.1.WLANConfiguration.[1-$max_num].Enable)
	freecwmp_parse_formated_parameter "$1.$i." "InternetGatewayDevice.LANDevice.1.WLANConfiguration.{i}.Enable" "rc" "num"
	if [ $rc -eq 0 ] && [ $num -gt 0 ] && [ $num -le $max_num ]; then
		let uci_num=$num-1
		get_wlan_enable $uci_num
	fi
	return $FAULT_CPE_NO_FAULT
	;;
	InternetGatewayDevice.LANDevice.1.WLANConfiguration.[1-$max_num].Status)
	freecwmp_parse_formated_parameter "$1" "InternetGatewayDevice.LANDevice.1.WLANConfiguration.{i}.Status" "rc" "num"
	if [ $rc -eq 0 ] && [ $num -gt 0 ] && [ $num -le $max_num ]; then
		let uci_num=$num-1
		get_wlan_status $uci_num
	fi
	return $FAULT_CPE_NO_FAULT
	;;
	InternetGatewayDevice.LANDevice.1.WLANConfiguration.[1-$max_num].BSSID)
	freecwmp_parse_formated_parameter "$1" "InternetGatewayDevice.LANDevice.1.WLANConfiguration.{i}.BSSID" "rc" "num"
	if [ $rc -eq 0 ] && [ $num -gt 0 ] && [ $num -le $max_num ]; then
		let uci_num=$num-1
		get_wlan_bssid $uci_num
	fi
	return $FAULT_CPE_NO_FAULT
	;;	
	InternetGatewayDevice.LANDevice.1.WLANConfiguration.[1-$max_num].MaxBitRate)
	freecwmp_parse_formated_parameter "$1" "InternetGatewayDevice.LANDevice.1.WLANConfiguration.{i}.MaxBitRate" "rc" "num"
	if [ $rc -eq 0 ] && [ $num -gt 0 ] && [ $num -le $max_num ]; then
		let uci_num=$num-1
		get_wlan_max_bit_rate $uci_num
	fi
	return $FAULT_CPE_NO_FAULT
	;;
	InternetGatewayDevice.LANDevice.1.WLANConfiguration.[1-$max_num].Channel)
	freecwmp_parse_formated_parameter "$1" "InternetGatewayDevice.LANDevice.1.WLANConfiguration.{i}.Channel" "rc" "num"
	if [ $rc -eq 0 ] && [ $num -gt 0 ] && [ $num -le $max_num ]; then
		let uci_num=$num-1
		get_wlan_channel $uci_num
	fi
	return $FAULT_CPE_NO_FAULT
	;;
	InternetGatewayDevice.LANDevice.1.WLANConfiguration.[1-$max_num].SSID)
	freecwmp_parse_formated_parameter "$1" "InternetGatewayDevice.LANDevice.1.WLANConfiguration.{i}.SSID" "rc" "num"
	if [ $rc -eq 0 ] && [ $num -gt 0 ] && [ $num -le $max_num ]; then
		let uci_num=$num-1
		get_wlan_ssid $uci_num
	fi
	return $FAULT_CPE_NO_FAULT
	;;
	InternetGatewayDevice.LANDevice.1.WLANConfiguration.[1-$max_num].BeaconType)
	freecwmp_parse_formated_parameter "$1" "InternetGatewayDevice.LANDevice.1.WLANConfiguration.{i}.BeaconType" "rc" "num"
	if [ $rc -eq 0 ] && [ $num -gt 0 ] && [ $num -le $max_num ]; then
		let uci_num=$num-1
		get_wlan_beacon_type $uci_num
	fi
	return $FAULT_CPE_NO_FAULT
	;;
	InternetGatewayDevice.LANDevice.1.WLANConfiguration.[1-$max_num].MACAddressControlEnabled)
	freecwmp_parse_formated_parameter "$1" "InternetGatewayDevice.LANDevice.1.WLANConfiguration.{i}.MACAddressControlEnabled" "rc" "num"
	if [ $rc -eq 0 ] && [ $num -gt 0 ] && [ $num -le $max_num ]; then
		let uci_num=$num-1
		get_wlan_mac_control_enable $uci_num
	fi
	return $FAULT_CPE_NO_FAULT
	;;
	InternetGatewayDevice.LANDevice.1.WLANConfiguration.[1-$max_num].Standard)
	freecwmp_parse_formated_parameter "$1" "InternetGatewayDevice.LANDevice.1.WLANConfiguration.{i}.Standard" "rc" "num"
	if [ $rc -eq 0 ] && [ $num -gt 0 ] && [ $num -le $max_num ]; then
		let uci_num=$num-1
		get_wlan_standard $uci_num
	fi
	return $FAULT_CPE_NO_FAULT
	;;
	InternetGatewayDevice.LANDevice.1.WLANConfiguration.[1-$max_num].WEPKeyIndex)
	freecwmp_parse_formated_parameter "$1" "InternetGatewayDevice.LANDevice.1.WLANConfiguration.{i}.WEPKeyIndex" "rc" "num"
	if [ $rc -eq 0 ] && [ $num -gt 0 ] && [ $num -le $max_num ]; then
		let uci_num=$num-1
		get_wlan_wep_key_index $uci_num
	fi
	return $FAULT_CPE_NO_FAULT
	;;
	InternetGatewayDevice.LANDevice.1.WLANConfiguration.[1-$max_num].KeyPassphrase)
	freecwmp_parse_formated_parameter "$1" "InternetGatewayDevice.LANDevice.1.WLANConfiguration.{i}.KeyPassphrase" "rc" "num"
	if [ $rc -eq 0 ] && [ $num -gt 0 ] && [ $num -le $max_num ]; then
		let uci_num=$num-1
		get_wlan_key_passphrase $uci_num
	fi
	return $FAULT_CPE_NO_FAULT
	;;
	InternetGatewayDevice.LANDevice.1.WLANConfiguration.[1-$max_num].WEPEncryptionLevel)
	freecwmp_parse_formated_parameter "$1" "InternetGatewayDevice.LANDevice.1.WLANConfiguration.{i}.WEPEncryptionLevel" "rc" "num"
	if [ $rc -eq 0 ] && [ $num -gt 0 ] && [ $num -le $max_num ]; then
		let uci_num=$num-1
		get_wlan_wep_encryption_level $uci_num
	fi
	return $FAULT_CPE_NO_FAULT
	;;
	InternetGatewayDevice.LANDevice.1.WLANConfiguration.[1-$max_num].BasicEncryptionModes)
	freecwmp_parse_formated_parameter "$1" "InternetGatewayDevice.LANDevice.1.WLANConfiguration.{i}.BasicEncryptionModes" "rc" "num"
	if [ $rc -eq 0 ] && [ $num -gt 0 ] && [ $num -le $max_num ]; then
		let uci_num=$num-1
		get_wlan_basic_encryption_modes $uci_num
	fi
	return $FAULT_CPE_NO_FAULT
	;;
	InternetGatewayDevice.LANDevice.1.WLANConfiguration.[1-$max_num].BasicAuthenticationMode)
	freecwmp_parse_formated_parameter "$1" "InternetGatewayDevice.LANDevice.1.WLANConfiguration.{i}.BasicAuthenticationMode" "rc" "num"
	if [ $rc -eq 0 ] && [ $num -gt 0 ] && [ $num -le $max_num ]; then
		let uci_num=$num-1
		get_wlan_basic_authentication_mode $uci_num
	fi
	return $FAULT_CPE_NO_FAULT
	;;
	InternetGatewayDevice.LANDevice.1.WLANConfiguration.[1-$max_num].WPAEncryptionModes)
	freecwmp_parse_formated_parameter "$1" "InternetGatewayDevice.LANDevice.1.WLANConfiguration.{i}.WPAEncryptionModes" "rc" "num"
	if [ $rc -eq 0 ] && [ $num -gt 0 ] && [ $num -le $max_num ]; then
		let uci_num=$num-1
		get_wlan_wpa_encryption_modes $uci_num
	fi
	return $FAULT_CPE_NO_FAULT
	;;
	InternetGatewayDevice.LANDevice.1.WLANConfiguration.[1-$max_num].WPAAuthenticationMode)
	freecwmp_parse_formated_parameter "$1" "InternetGatewayDevice.LANDevice.1.WLANConfiguration.{i}.WPAAuthenticationMode" "rc" "num"
	if [ $rc -eq 0 ] && [ $num -gt 0 ] && [ $num -le $max_num ]; then
		let uci_num=$num-1
		get_wlan_wpa_authentication_mode $uci_num
	fi
	return $FAULT_CPE_NO_FAULT
	;;
	InternetGatewayDevice.LANDevice.1.WLANConfiguration.[1-$max_num].IEEE11iEncryptionModes)
	freecwmp_parse_formated_parameter "$1" "InternetGatewayDevice.LANDevice.1.WLANConfiguration.{i}.IEEE11iEncryptionModes" "rc" "num"
	if [ $rc -eq 0 ] && [ $num -gt 0 ] && [ $num -le $max_num ]; then
		let uci_num=$num-1
		get_wlan_ieee_11i_encryption_modes $uci_num
	fi
	return $FAULT_CPE_NO_FAULT
	;;
	InternetGatewayDevice.LANDevice.1.WLANConfiguration.[1-$max_num].IEEE11iAuthenticationMode)
	freecwmp_parse_formated_parameter "$1" "InternetGatewayDevice.LANDevice.1.WLANConfiguration.{i}.IEEE11iAuthenticationMode" "rc" "num"
	if [ $rc -eq 0 ] && [ $num -gt 0 ] && [ $num -le $max_num ]; then
		let uci_num=$num-1
		get_wlan_ieee_11i_authentication_mode $uci_num
	fi
	return $FAULT_CPE_NO_FAULT
	;;	
	InternetGatewayDevice.LANDevice.1.WLANConfiguration.[1-$max_num].DeviceOperationMode)
	freecwmp_parse_formated_parameter "$1" "InternetGatewayDevice.LANDevice.1.WLANConfiguration.{i}.DeviceOperationMode" "rc" "num"
	if [ $rc -eq 0 ] && [ $num -gt 0 ] && [ $num -le $max_num ]; then
		let uci_num=$num-1
		get_wlan_device_operation_mode $uci_num
	fi
	return $FAULT_CPE_NO_FAULT
	;;	
	InternetGatewayDevice.LANDevice.1.WLANConfiguration.[1-$max_num].AuthenticationServiceMode)
	freecwmp_parse_formated_parameter "$1" "InternetGatewayDevice.LANDevice.1.WLANConfiguration.{i}.AuthenticationServiceMode" "rc" "num"
	if [ $rc -eq 0 ] && [ $num -gt 0 ] && [ $num -le $max_num ]; then
		let uci_num=$num-1
		get_wlan_authentication_service_mode $uci_num
	fi
	return $FAULT_CPE_NO_FAULT
	;;
	InternetGatewayDevice.LANDevice.1.WLANConfiguration.[1-$max_num].RadioEnabled)
	freecwmp_parse_formated_parameter "$1" "InternetGatewayDevice.LANDevice.1.WLANConfiguration.{i}.RadioEnabled" "rc" "num"
	if [ $rc -eq 0 ] && [ $num -gt 0 ] && [ $num -le $max_num ]; then
		let uci_num=$num-1
		get_wlan_radio_enabled $uci_num
	fi
	return $FAULT_CPE_NO_FAULT
	;;
	InternetGatewayDevice.LANDevice.1.WLANConfiguration.[1-$max_num].TotalAssociations)
	freecwmp_parse_formated_parameter "$1" "InternetGatewayDevice.LANDevice.1.WLANConfiguration.{i}.TotalAssociations" "rc" "num"
	if [ $rc -eq 0 ] && [ $num -gt 0 ] && [ $num -le $max_num ]; then
		let uci_num=$num-1
		get_wlan_total_associations $uci_num
	fi
	return $FAULT_CPE_NO_FAULT
	;;	
	InternetGatewayDevice.LANDevice.1.WLANConfiguration.[1-$max_num].WEPKey.1.WEPKey)
	freecwmp_parse_formated_parameter "$1" "InternetGatewayDevice.LANDevice.1.WLANConfiguration.{i}.WEPKey.1.WEPKey" "rc" "num"
	if [ $rc -eq 0 ] && [ $num -gt 0 ] && [ $num -le $max_num ]; then
		let uci_num=$num-1
		get_wlan_wep_key $uci_num "1"
	fi
	return $FAULT_CPE_NO_FAULT
	;;
	InternetGatewayDevice.LANDevice.1.WLANConfiguration.[1-$max_num].WEPKey.2.WEPKey)
	freecwmp_parse_formated_parameter "$1" "InternetGatewayDevice.LANDevice.1.WLANConfiguration.{i}.WEPKey.2.WEPKey" "rc" "num"
	if [ $rc -eq 0 ] && [ $num -gt 0 ] && [ $num -le $max_num ]; then
		let uci_num=$num-1
		get_wlan_wep_key $uci_num "2"
	fi
	return $FAULT_CPE_NO_FAULT
	;;
	InternetGatewayDevice.LANDevice.1.WLANConfiguration.[1-$max_num].WEPKey.3.WEPKey)
	freecwmp_parse_formated_parameter "$1" "InternetGatewayDevice.LANDevice.1.WLANConfiguration.{i}.WEPKey.3.WEPKey" "rc" "num"
	if [ $rc -eq 0 ] && [ $num -gt 0 ] && [ $num -le $max_num ]; then
		let uci_num=$num-1
		get_wlan_wep_key $uci_num "3"
	fi
	return $FAULT_CPE_NO_FAULT
	;;
	InternetGatewayDevice.LANDevice.1.WLANConfiguration.[1-$max_num].WEPKey.4.WEPKey)
	freecwmp_parse_formated_parameter "$1" "InternetGatewayDevice.LANDevice.1.WLANConfiguration.{i}.WEPKey.4.WEPKey" "rc" "num"
	if [ $rc -eq 0 ] && [ $num -gt 0 ] && [ $num -le $max_num ]; then
		let uci_num=$num-1
		get_wlan_wep_key $uci_num "4"
	fi
	return $FAULT_CPE_NO_FAULT
	;;
	InternetGatewayDevice.LANDevice.1.WLANConfiguration.[1-$max_num].PreSharedKey.1.PreSharedKey)
	freecwmp_parse_formated_parameter "$1" "InternetGatewayDevice.LANDevice.1.WLANConfiguration.{i}.PreSharedKey.1.PreSharedKey" "rc" "num"
	if [ $rc -eq 0 ] && [ $num -gt 0 ] && [ $num -le $max_num ]; then
		let uci_num=$num-1
		get_wlan_pre_shared_key $uci_num "1"
	fi
	return $FAULT_CPE_NO_FAULT
	;;
	InternetGatewayDevice.LANDevice.1.WLANConfiguration.[1-$max_num].PreSharedKey.1.KeyPassphrase)
	freecwmp_parse_formated_parameter "$1" "InternetGatewayDevice.LANDevice.1.WLANConfiguration.{i}.PreSharedKey.1.KeyPassphrase" "rc" "num"
	if [ $rc -eq 0 ] && [ $num -gt 0 ] && [ $num -le $max_num ]; then
		let uci_num=$num-1
		get_wlan_pre_shared_key_key_passphrase $uci_num "1"
	fi
	return $FAULT_CPE_NO_FAULT
	;;
	InternetGatewayDevice.LANDevice.1.WLANConfiguration.[1-$max_num].PreSharedKey.1.AssociatedDeviceMACAddress)
	freecwmp_parse_formated_parameter "$1" "InternetGatewayDevice.LANDevice.1.WLANConfiguration.{i}.PreSharedKey.1.AssociatedDeviceMACAddress" "rc" "num"
	if [ $rc -eq 0 ] && [ $num -gt 0 ] && [ $num -le $max_num ]; then
		let uci_num=$num-1
		get_wlan_pre_shared_key_associated_device_MACAddress $uci_num "1"
	fi
	return $FAULT_CPE_NO_FAULT
	;;
esac
return $FAULT_CPE_INVALID_PARAMETER_NAME
}

set_lan_device() {
local max_num

get_wlan_number_of_entries "max_num"

case "$1" in
	InternetGatewayDevice.LANDevice.1.WLANConfiguration.[1-$max_num].Enable)
	freecwmp_parse_formated_parameter "$1" "InternetGatewayDevice.LANDevice.1.WLANConfiguration.{i}.Enable" "rc" "num"
	if [ $rc -eq 0 ] && [ $num -gt 0 ] && [ $num -le $max_num ]; then
		let uci_num=$num-1
		set_wlan_enable $uci_num "$2"
	fi
	return $FAULT_CPE_NO_FAULT
	;;
	InternetGatewayDevice.LANDevice.1.WLANConfiguration.[1-$max_num].Status)
	return $FAULT_CPE_NON_WRITABLE_PARAMETER
	;;
	InternetGatewayDevice.LANDevice.1.WLANConfiguration.[1-$max_num].BSSID)
	return $FAULT_CPE_NON_WRITABLE_PARAMETER
	;;
	InternetGatewayDevice.LANDevice.1.WLANConfiguration.[1-$max_num].MaxBitRate)
	freecwmp_parse_formated_parameter "$1" "InternetGatewayDevice.LANDevice.1.WLANConfiguration.{i}.MaxBitRate" "rc" "num"
	if [ $rc -eq 0 ] && [ $num -gt 0 ] && [ $num -le $max_num ]; then
		let uci_num=$num-1
		set_wlan_max_bit_rate $uci_num "$2"
	fi
	return $FAULT_CPE_NO_FAULT
	;;
	InternetGatewayDevice.LANDevice.1.WLANConfiguration.[1-$max_num].Channel)
	if [ $2 -lt 0 -o $2 -gt 255 ];then
		return $FAULT_CPE_INVALID_PARAMETER_VALUE
	fi
	freecwmp_parse_formated_parameter "$1" "InternetGatewayDevice.LANDevice.1.WLANConfiguration.{i}.Channel" "rc" "num"
	if [ $rc -eq 0 ] && [ $num -gt 0 ] && [ $num -le $max_num ]; then
		let uci_num=$num-1
		set_wlan_channel $uci_num "$2"
	fi
	return $FAULT_CPE_NO_FAULT
	;;
	InternetGatewayDevice.LANDevice.1.WLANConfiguration.[1-$max_num].SSID)
	freecwmp_parse_formated_parameter "$1" "InternetGatewayDevice.LANDevice.1.WLANConfiguration.{i}.SSID" "rc" "num"
	if [ $rc -eq 0 ] && [ $num -gt 0 ] && [ $num -le $max_num ]; then
		let uci_num=$num-1
		set_wlan_ssid $uci_num "$2"
	fi
	return $FAULT_CPE_NO_FAULT
	;;
	InternetGatewayDevice.LANDevice.1.WLANConfiguration.[1-$max_num].BeaconType)
	freecwmp_parse_formated_parameter "$1" "InternetGatewayDevice.LANDevice.1.WLANConfiguration.{i}.BeaconType" "rc" "num"
	if [ $rc -eq 0 ] && [ $num -gt 0 ] && [ $num -le $max_num ]; then
		let uci_num=$num-1
		set_wlan_beacon_type $uci_num "$2"
	fi
	return $FAULT_CPE_NO_FAULT
	;;
	InternetGatewayDevice.LANDevice.1.WLANConfiguration.[1-$max_num].MACAddressControlEnabled)
	freecwmp_parse_formated_parameter "$1" "InternetGatewayDevice.LANDevice.1.WLANConfiguration.{i}.MACAddressControlEnabled" "rc" "num"
	if [ $rc -eq 0 ] && [ $num -gt 0 ] && [ $num -le $max_num ]; then
		let uci_num=$num-1
		set_wlan_mac_control_enable $uci_num "$2"
	fi
	return $FAULT_CPE_NO_FAULT
	;;
	InternetGatewayDevice.LANDevice.1.WLANConfiguration.[1-$max_num].Standard)
	freecwmp_parse_formated_parameter "$1" "InternetGatewayDevice.LANDevice.1.WLANConfiguration.{i}.Standard" "rc" "num"
	if [ $rc -eq 0 ] && [ $num -gt 0 ] && [ $num -le $max_num ]; then
		let uci_num=$num-1
		set_wlan_standard $uci_num "$2"
	fi
	return $FAULT_CPE_NO_FAULT
	;;
	InternetGatewayDevice.LANDevice.1.WLANConfiguration.[1-$max_num].WEPKeyIndex)
	freecwmp_parse_formated_parameter "$1" "InternetGatewayDevice.LANDevice.1.WLANConfiguration.{i}.WEPKeyIndex" "rc" "num"
	if [ $rc -eq 0 ] && [ $num -gt 0 ] && [ $num -le $max_num ]; then
		let uci_num=$num-1
		set_wlan_wep_key_index $uci_num "$2"
	fi
	return $FAULT_CPE_NO_FAULT
	;;
	InternetGatewayDevice.LANDevice.1.WLANConfiguration.[1-$max_num].KeyPassphrase)
	freecwmp_parse_formated_parameter "$1" "InternetGatewayDevice.LANDevice.1.WLANConfiguration.{i}.KeyPassphrase" "rc" "num"
	if [ $rc -eq 0 ] && [ $num -gt 0 ] && [ $num -le $max_num ]; then
		let uci_num=$num-1
		set_wlan_key_passphrase $uci_num "$2"
	fi
	return $FAULT_CPE_NO_FAULT
	;;
	InternetGatewayDevice.LANDevice.1.WLANConfiguration.[1-$max_num].WEPEncryptionLevel)
	return $FAULT_CPE_NON_WRITABLE_PARAMETER
	;;
	InternetGatewayDevice.LANDevice.1.WLANConfiguration.[1-$max_num].BasicEncryptionModes)
	freecwmp_parse_formated_parameter "$1" "InternetGatewayDevice.LANDevice.1.WLANConfiguration.{i}.BasicEncryptionModes" "rc" "num"
	if [ $rc -eq 0 ] && [ $num -gt 0 ] && [ $num -le $max_num ]; then
		let uci_num=$num-1
		set_wlan_basic_encryption_modes $uci_num "$2"
	fi
	return $FAULT_CPE_NO_FAULT
	;;
	InternetGatewayDevice.LANDevice.1.WLANConfiguration.[1-$max_num].BasicAuthenticationMode)
	freecwmp_parse_formated_parameter "$1" "InternetGatewayDevice.LANDevice.1.WLANConfiguration.{i}.BasicAuthenticationMode" "rc" "num"
	if [ $rc -eq 0 ] && [ $num -gt 0 ] && [ $num -le $max_num ]; then
		let uci_num=$num-1
		set_wlan_basic_authentication_mode $uci_num "$2"
	fi
	return $FAULT_CPE_NO_FAULT
	;;
	InternetGatewayDevice.LANDevice.1.WLANConfiguration.[1-$max_num].WPAEncryptionModes)
	freecwmp_parse_formated_parameter "$1" "InternetGatewayDevice.LANDevice.1.WLANConfiguration.{i}.WPAEncryptionModes" "rc" "num"
	if [ $rc -eq 0 ] && [ $num -gt 0 ] && [ $num -le $max_num ]; then
		let uci_num=$num-1
		set_wlan_wpa_encryption_modes $uci_num "$2"
	fi
	return $FAULT_CPE_NO_FAULT
	;;
	InternetGatewayDevice.LANDevice.1.WLANConfiguration.[1-$max_num].WPAAuthenticationMode)
	freecwmp_parse_formated_parameter "$1" "InternetGatewayDevice.LANDevice.1.WLANConfiguration.{i}.WPAAuthenticationMode" "rc" "num"
	if [ $rc -eq 0 ] && [ $num -gt 0 ] && [ $num -le $max_num ]; then
		let uci_num=$num-1
		set_wlan_wpa_authentication_mode $uci_num "$2"
	fi
	return $FAULT_CPE_NO_FAULT
	;;
	InternetGatewayDevice.LANDevice.1.WLANConfiguration.[1-$max_num].IEEE11iEncryptionModes)
	freecwmp_parse_formated_parameter "$1" "InternetGatewayDevice.LANDevice.1.WLANConfiguration.{i}.IEEE11iEncryptionModes" "rc" "num"
	if [ $rc -eq 0 ] && [ $num -gt 0 ] && [ $num -le $max_num ]; then
		let uci_num=$num-1
		set_wlan_ieee_11i_encryption_modes $uci_num "$2"
	fi
	return $FAULT_CPE_NO_FAULT
	;;
	InternetGatewayDevice.LANDevice.1.WLANConfiguration.[1-$max_num].IEEE11iAuthenticationMode)
	freecwmp_parse_formated_parameter "$1" "InternetGatewayDevice.LANDevice.1.WLANConfiguration.{i}.IEEE11iAuthenticationMode" "rc" "num"
	if [ $rc -eq 0 ] && [ $num -gt 0 ] && [ $num -le $max_num ]; then
		let uci_num=$num-1
		set_wlan_ieee_11i_authentication_mode $uci_num "$2"
	fi
	return $FAULT_CPE_NO_FAULT
	;;
	InternetGatewayDevice.LANDevice.1.WLANConfiguration.[1-$max_num].RadioEnabled)
	freecwmp_parse_formated_parameter "$1" "InternetGatewayDevice.LANDevice.1.WLANConfiguration.{i}.RadioEnabled" "rc" "num"
	if [ $rc -eq 0 ] && [ $num -gt 0 ] && [ $num -le $max_num ]; then
		let uci_num=$num-1
		set_wlan_radio_enabled $uci_num "$2"
	fi
	return $FAULT_CPE_NO_FAULT
	;;
	InternetGatewayDevice.LANDevice.1.WLANConfiguration.[1-$max_num].DeviceOperationMode)
	freecwmp_parse_formated_parameter "$1" "InternetGatewayDevice.LANDevice.1.WLANConfiguration.{i}.DeviceOperationMode" "rc" "num"
	if [ $rc -eq 0 ] && [ $num -gt 0 ] && [ $num -le $max_num ]; then
		let uci_num=$num-1
		set_wlan_device_operation_mode $uci_num "$2"
	fi
	return $FAULT_CPE_NO_FAULT
	;;
	InternetGatewayDevice.LANDevice.1.WLANConfiguration.[1-$max_num].AuthenticationServiceMode)
	freecwmp_parse_formated_parameter "$1" "InternetGatewayDevice.LANDevice.1.WLANConfiguration.{i}.AuthenticationServiceMode" "rc" "num"
	if [ $rc -eq 0 ] && [ $num -gt 0 ] && [ $num -le $max_num ]; then
		let uci_num=$num-1
		set_wlan_authentication_service_mode $uci_num "$2"
	fi
	return $FAULT_CPE_NO_FAULT
	;;
	InternetGatewayDevice.LANDevice.1.WLANConfiguration.[1-$max_num].TotalAssociations)
	return $FAULT_CPE_NON_WRITABLE_PARAMETER
	;;
	InternetGatewayDevice.LANDevice.1.WLANConfiguration.[1-$max_num].WEPKey.1.WEPKey)
	freecwmp_parse_formated_parameter "$1" "InternetGatewayDevice.LANDevice.1.WLANConfiguration.{i}.WEPKey.1.WEPKey" "rc" "num"
	if [ $rc -eq 0 ] && [ $num -gt 0 ] && [ $num -le $max_num ]; then
		let uci_num=$num-1
		set_wlan_wep_key $uci_num "1" "$2"
	fi
	return $FAULT_CPE_NO_FAULT
	;;
	InternetGatewayDevice.LANDevice.1.WLANConfiguration.[1-$max_num].WEPKey.2.WEPKey)
	freecwmp_parse_formated_parameter "$1" "InternetGatewayDevice.LANDevice.1.WLANConfiguration.{i}.WEPKey.2.WEPKey" "rc" "num"
	if [ $rc -eq 0 ] && [ $num -gt 0 ] && [ $num -le $max_num ]; then
		let uci_num=$num-1
		set_wlan_wep_key $uci_num "2" "$2"
	fi
	return $FAULT_CPE_NO_FAULT
	;;
	InternetGatewayDevice.LANDevice.1.WLANConfiguration.[1-$max_num].WEPKey.3.WEPKey)
	freecwmp_parse_formated_parameter "$1" "InternetGatewayDevice.LANDevice.1.WLANConfiguration.{i}.WEPKey.3.WEPKey" "rc" "num"
	if [ $rc -eq 0 ] && [ $num -gt 0 ] && [ $num -le $max_num ]; then
		let uci_num=$num-1
		set_wlan_wep_key $uci_num "3" "$2"
	fi
	return $FAULT_CPE_NO_FAULT
	;;
	InternetGatewayDevice.LANDevice.1.WLANConfiguration.[1-$max_num].WEPKey.4.WEPKey)
	freecwmp_parse_formated_parameter "$1" "InternetGatewayDevice.LANDevice.1.WLANConfiguration.{i}.WEPKey.4.WEPKey" "rc" "num"
	if [ $rc -eq 0 ] && [ $num -gt 0 ] && [ $num -le $max_num ]; then
		let uci_num=$num-1
		set_wlan_wep_key $uci_num "4" "$2"
	fi
	return $FAULT_CPE_NO_FAULT
	;;
	InternetGatewayDevice.LANDevice.1.WLANConfiguration.[1-$max_num].PreSharedKey.1.PreSharedKey)
	freecwmp_parse_formated_parameter "$1" "InternetGatewayDevice.LANDevice.1.WLANConfiguration.{i}.PreSharedKey.1.PreSharedKey" "rc" "num"
	if [ $rc -eq 0 ] && [ $num -gt 0 ] && [ $num -le $max_num ]; then
		let uci_num=$num-1
		set_wlan_pre_shared_key $uci_num "1" "$2"
	fi
	return $FAULT_CPE_NO_FAULT
	;;
	InternetGatewayDevice.LANDevice.1.WLANConfiguration.[1-$max_num].PreSharedKey.1.KeyPassphrase)
	freecwmp_parse_formated_parameter "$1" "InternetGatewayDevice.LANDevice.1.WLANConfiguration.{i}.PreSharedKey.1.KeyPassphrase" "rc" "num"
	if [ $rc -eq 0 ] && [ $num -gt 0 ] && [ $num -le $max_num ]; then
		let uci_num=$num-1
		set_wlan_pre_shared_key_key_passphrase $uci_num "1" "$2"
	fi
	return $FAULT_CPE_NO_FAULT
	;;
	InternetGatewayDevice.LANDevice.1.WLANConfiguration.[1-$max_num].PreSharedKey.1.AssociatedDeviceMACAddress)
	freecwmp_parse_formated_parameter "$1" "InternetGatewayDevice.LANDevice.1.WLANConfiguration.{i}.PreSharedKey.1.AssociatedDeviceMACAddress" "rc" "num"
	if [ $rc -eq 0 ] && [ $num -gt 0 ] && [ $num -le $max_num ]; then
		let uci_num=$num-1
		set_wlan_pre_shared_key_associated_device_MACAddress $uci_num "1" "$2"
	fi
	return $FAULT_CPE_NO_FAULT
	;;
	*)
	set_ip_interface_ip_address $1 $2
	result1=$?
	set_ip_interface_netmask $1 $2
	result2=$?
	if [ \( "$result1" != "$FAULT_CPE_NO_FAULT" \) -a \( "$result2" != "$FAULT_CPE_NO_FAULT" \) ]; then
		return $FAULT_CPE_INVALID_PARAMETER_NAME
	fi
	return $FAULT_CPE_NO_FAULT
	;;
esac
return $FAULT_CPE_INVALID_PARAMETER_NAME
}

set_lan_device_notification() {
local max_num

get_wlan_number_of_entries "max_num"

case "$1" in
	InternetGatewayDevice.)
	freecwmp_set_parameter_notification "InternetGatewayDevice.LANDevice.1." "0"
	freecwmp_set_parameter_notification "InternetGatewayDevice.LANDevice.1.WLANConfiguration." "0"
	for i in `seq 1 $max_num`;do
		freecwmp_parse_formated_parameter $1"LANDevice.1.WLANConfiguration.$i." "InternetGatewayDevice.LANDevice.1.WLANConfiguration.{i}." "rc" "num"
		if [ $rc -eq 0 ] && [ $num -gt 0 ] && [ $num -le $max_num ]; then
			freecwmp_set_parameter_notification "InternetGatewayDevice.LANDevice.1.WLANConfiguration.$i." "0"
			freecwmp_set_parameter_notification "InternetGatewayDevice.LANDevice.1.WLANConfiguration.$i.Enable" "0"
			freecwmp_set_parameter_notification "InternetGatewayDevice.LANDevice.1.WLANConfiguration.$i.MaxBitRate" "0"
			freecwmp_set_parameter_notification "InternetGatewayDevice.LANDevice.1.WLANConfiguration.$i.Channel" "0"
			freecwmp_set_parameter_notification "InternetGatewayDevice.LANDevice.1.WLANConfiguration.$i.SSID" "0"
			freecwmp_set_parameter_notification "InternetGatewayDevice.LANDevice.1.WLANConfiguration.$i.BeaconType" "0"
			freecwmp_set_parameter_notification "InternetGatewayDevice.LANDevice.1.WLANConfiguration.$i.MACAddressControlEnabled" "0"
			freecwmp_set_parameter_notification "InternetGatewayDevice.LANDevice.1.WLANConfiguration.$i.Standard" "0"
			freecwmp_set_parameter_notification "InternetGatewayDevice.LANDevice.1.WLANConfiguration.$i.WEPKeyIndex" "0"
			freecwmp_set_parameter_notification "InternetGatewayDevice.LANDevice.1.WLANConfiguration.$i.KeyPassphrase" "0"
			freecwmp_set_parameter_notification "InternetGatewayDevice.LANDevice.1.WLANConfiguration.$i.WEPEncryptionLevel" "0"
			freecwmp_set_parameter_notification "InternetGatewayDevice.LANDevice.1.WLANConfiguration.$i.BasicEncryptionModes" "0"
			freecwmp_set_parameter_notification "InternetGatewayDevice.LANDevice.1.WLANConfiguration.$i.BasicAuthenticationMode" "0"
			freecwmp_set_parameter_notification "InternetGatewayDevice.LANDevice.1.WLANConfiguration.$i.WPAEncryptionModes" "0"
			freecwmp_set_parameter_notification "InternetGatewayDevice.LANDevice.1.WLANConfiguration.$i.WPAAuthenticationMode" "0"
			freecwmp_set_parameter_notification "InternetGatewayDevice.LANDevice.1.WLANConfiguration.$i.IEEE11iEncryptionModes" "0"
			freecwmp_set_parameter_notification "InternetGatewayDevice.LANDevice.1.WLANConfiguration.$i.IEEE11iAuthenticationMode" "0"
			freecwmp_set_parameter_notification "InternetGatewayDevice.LANDevice.1.WLANConfiguration.$i.RadioEnabled" "0"
			freecwmp_set_parameter_notification "InternetGatewayDevice.LANDevice.1.WLANConfiguration.$i.DeviceOperationMode" "0"
			freecwmp_set_parameter_notification "InternetGatewayDevice.LANDevice.1.WLANConfiguration.$i.AuthenticationServiceMode" "0"
			freecwmp_set_parameter_notification "InternetGatewayDevice.LANDevice.1.WLANConfiguration.$i.WEPKey.1.WEPKey" "0"
			freecwmp_set_parameter_notification "InternetGatewayDevice.LANDevice.1.WLANConfiguration.$i.WEPKey.2.WEPKey" "0"
			freecwmp_set_parameter_notification "InternetGatewayDevice.LANDevice.1.WLANConfiguration.$i.WEPKey.3.WEPKey" "0"
			freecwmp_set_parameter_notification "InternetGatewayDevice.LANDevice.1.WLANConfiguration.$i.WEPKey.4.WEPKey" "0"
			freecwmp_set_parameter_notification "InternetGatewayDevice.LANDevice.1.WLANConfiguration.$i.PreSharedKey.1.PreSharedKey" "0"
			freecwmp_set_parameter_notification "InternetGatewayDevice.LANDevice.1.WLANConfiguration.$i.PreSharedKey.1.KeyPassphrase" "0"
			freecwmp_set_parameter_notification "InternetGatewayDevice.LANDevice.1.WLANConfiguration.$i.PreSharedKey.1.AssociatedDeviceMACAddress" "0"
		fi
	done
	return $FAULT_CPE_NO_FAULT
	;;
	InternetGatewayDevice.LANDevice.)
	freecwmp_set_parameter_notification "InternetGatewayDevice.LANDevice.1." "0"
	freecwmp_set_parameter_notification "InternetGatewayDevice.LANDevice.1.WLANConfiguration." "0"
	for i in `seq 1 $max_num`;do
		freecwmp_parse_formated_parameter $1"1.WLANConfiguration.$i." "InternetGatewayDevice.LANDevice.1.WLANConfiguration.{i}." "rc" "num"
		if [ $rc -eq 0 ] && [ $num -gt 0 ] && [ $num -le $max_num ]; then
			let uci_num=$num-1
			freecwmp_set_parameter_notification "InternetGatewayDevice.LANDevice.1.WLANConfiguration.$i." "0"
			freecwmp_set_parameter_notification "InternetGatewayDevice.LANDevice.1.WLANConfiguration.$i.Enable" "0"
			freecwmp_set_parameter_notification "InternetGatewayDevice.LANDevice.1.WLANConfiguration.$i.MaxBitRate" "0"
			freecwmp_set_parameter_notification "InternetGatewayDevice.LANDevice.1.WLANConfiguration.$i.Channel" "0"
			freecwmp_set_parameter_notification "InternetGatewayDevice.LANDevice.1.WLANConfiguration.$i.SSID" "0"
			freecwmp_set_parameter_notification "InternetGatewayDevice.LANDevice.1.WLANConfiguration.$i.BeaconType" "0"
			freecwmp_set_parameter_notification "InternetGatewayDevice.LANDevice.1.WLANConfiguration.$i.MACAddressControlEnabled" "0"
			freecwmp_set_parameter_notification "InternetGatewayDevice.LANDevice.1.WLANConfiguration.$i.Standard" "0"
			freecwmp_set_parameter_notification "InternetGatewayDevice.LANDevice.1.WLANConfiguration.$i.WEPKeyIndex" "0"
			freecwmp_set_parameter_notification "InternetGatewayDevice.LANDevice.1.WLANConfiguration.$i.KeyPassphrase" "0"
			freecwmp_set_parameter_notification "InternetGatewayDevice.LANDevice.1.WLANConfiguration.$i.WEPEncryptionLevel" "0"
			freecwmp_set_parameter_notification "InternetGatewayDevice.LANDevice.1.WLANConfiguration.$i.BasicEncryptionModes" "0"
			freecwmp_set_parameter_notification "InternetGatewayDevice.LANDevice.1.WLANConfiguration.$i.BasicAuthenticationMode" "0"
			freecwmp_set_parameter_notification "InternetGatewayDevice.LANDevice.1.WLANConfiguration.$i.WPAEncryptionModes" "0"
			freecwmp_set_parameter_notification "InternetGatewayDevice.LANDevice.1.WLANConfiguration.$i.WPAAuthenticationMode" "0"
			freecwmp_set_parameter_notification "InternetGatewayDevice.LANDevice.1.WLANConfiguration.$i.IEEE11iEncryptionModes" "0"
			freecwmp_set_parameter_notification "InternetGatewayDevice.LANDevice.1.WLANConfiguration.$i.IEEE11iAuthenticationMode" "0"
			freecwmp_set_parameter_notification "InternetGatewayDevice.LANDevice.1.WLANConfiguration.$i.RadioEnabled" "0"
			freecwmp_set_parameter_notification "InternetGatewayDevice.LANDevice.1.WLANConfiguration.$i.DeviceOperationMode" "0"
			freecwmp_set_parameter_notification "InternetGatewayDevice.LANDevice.1.WLANConfiguration.$i.AuthenticationServiceMode" "0"
			freecwmp_set_parameter_notification "InternetGatewayDevice.LANDevice.1.WLANConfiguration.$i.WEPKey.1.WEPKey" "0"
			freecwmp_set_parameter_notification "InternetGatewayDevice.LANDevice.1.WLANConfiguration.$i.WEPKey.2.WEPKey" "0"
			freecwmp_set_parameter_notification "InternetGatewayDevice.LANDevice.1.WLANConfiguration.$i.WEPKey.3.WEPKey" "0"
			freecwmp_set_parameter_notification "InternetGatewayDevice.LANDevice.1.WLANConfiguration.$i.WEPKey.4.WEPKey" "0"
			freecwmp_set_parameter_notification "InternetGatewayDevice.LANDevice.1.WLANConfiguration.$i.PreSharedKey.1.PreSharedKey" "0"
			freecwmp_set_parameter_notification "InternetGatewayDevice.LANDevice.1.WLANConfiguration.$i.PreSharedKey.1.KeyPassphrase" "0"
			freecwmp_set_parameter_notification "InternetGatewayDevice.LANDevice.1.WLANConfiguration.$i.PreSharedKey.1.AssociatedDeviceMACAddress" "0"
		fi
	done
	freecwmp_set_parameter_notification "$1" "$2"
	return $FAULT_CPE_NO_FAULT
	;;
	InternetGatewayDevice.LANDevice.1.)
	freecwmp_set_parameter_notification "InternetGatewayDevice.LANDevice.1.WLANConfiguration." "0"
	for i in `seq 1 $max_num`;do
		freecwmp_parse_formated_parameter $1"WLANConfiguration.$i." "InternetGatewayDevice.LANDevice.1.WLANConfiguration.{i}." "rc" "num"
		if [ $rc -eq 0 ] && [ $num -gt 0 ] && [ $num -le $max_num ]; then
			let uci_num=$num-1
			freecwmp_set_parameter_notification "InternetGatewayDevice.LANDevice.1.WLANConfiguration.$i." "0"
			freecwmp_set_parameter_notification "InternetGatewayDevice.LANDevice.1.WLANConfiguration.$i.Enable" "0"
			freecwmp_set_parameter_notification "InternetGatewayDevice.LANDevice.1.WLANConfiguration.$i.MaxBitRate" "0"
			freecwmp_set_parameter_notification "InternetGatewayDevice.LANDevice.1.WLANConfiguration.$i.Channel" "0"
			freecwmp_set_parameter_notification "InternetGatewayDevice.LANDevice.1.WLANConfiguration.$i.SSID" "0"
			freecwmp_set_parameter_notification "InternetGatewayDevice.LANDevice.1.WLANConfiguration.$i.BeaconType" "0"
			freecwmp_set_parameter_notification "InternetGatewayDevice.LANDevice.1.WLANConfiguration.$i.MACAddressControlEnabled" "0"
			freecwmp_set_parameter_notification "InternetGatewayDevice.LANDevice.1.WLANConfiguration.$i.Standard" "0"
			freecwmp_set_parameter_notification "InternetGatewayDevice.LANDevice.1.WLANConfiguration.$i.WEPKeyIndex" "0"
			freecwmp_set_parameter_notification "InternetGatewayDevice.LANDevice.1.WLANConfiguration.$i.KeyPassphrase" "0"
			freecwmp_set_parameter_notification "InternetGatewayDevice.LANDevice.1.WLANConfiguration.$i.WEPEncryptionLevel" "0"
			freecwmp_set_parameter_notification "InternetGatewayDevice.LANDevice.1.WLANConfiguration.$i.BasicEncryptionModes" "0"
			freecwmp_set_parameter_notification "InternetGatewayDevice.LANDevice.1.WLANConfiguration.$i.BasicAuthenticationMode" "0"
			freecwmp_set_parameter_notification "InternetGatewayDevice.LANDevice.1.WLANConfiguration.$i.WPAEncryptionModes" "0"
			freecwmp_set_parameter_notification "InternetGatewayDevice.LANDevice.1.WLANConfiguration.$i.WPAAuthenticationMode" "0"
			freecwmp_set_parameter_notification "InternetGatewayDevice.LANDevice.1.WLANConfiguration.$i.IEEE11iEncryptionModes" "0"
			freecwmp_set_parameter_notification "InternetGatewayDevice.LANDevice.1.WLANConfiguration.$i.IEEE11iAuthenticationMode" "0"
			freecwmp_set_parameter_notification "InternetGatewayDevice.LANDevice.1.WLANConfiguration.$i.RadioEnabled" "0"
			freecwmp_set_parameter_notification "InternetGatewayDevice.LANDevice.1.WLANConfiguration.$i.DeviceOperationMode" "0"
			freecwmp_set_parameter_notification "InternetGatewayDevice.LANDevice.1.WLANConfiguration.$i.AuthenticationServiceMode" "0"
			freecwmp_set_parameter_notification "InternetGatewayDevice.LANDevice.1.WLANConfiguration.$i.WEPKey.1.WEPKey" "0"
			freecwmp_set_parameter_notification "InternetGatewayDevice.LANDevice.1.WLANConfiguration.$i.WEPKey.2.WEPKey" "0"
			freecwmp_set_parameter_notification "InternetGatewayDevice.LANDevice.1.WLANConfiguration.$i.WEPKey.3.WEPKey" "0"
			freecwmp_set_parameter_notification "InternetGatewayDevice.LANDevice.1.WLANConfiguration.$i.WEPKey.4.WEPKey" "0"
			freecwmp_set_parameter_notification "InternetGatewayDevice.LANDevice.1.WLANConfiguration.$i.PreSharedKey.1.PreSharedKey" "0"
			freecwmp_set_parameter_notification "InternetGatewayDevice.LANDevice.1.WLANConfiguration.$i.PreSharedKey.1.KeyPassphrase" "0"
			freecwmp_set_parameter_notification "InternetGatewayDevice.LANDevice.1.WLANConfiguration.$i.PreSharedKey.1.AssociatedDeviceMACAddress" "0"
		fi
	done
	freecwmp_set_parameter_notification "$1" "$2"
	return $FAULT_CPE_NO_FAULT
	;;
	InternetGatewayDevice.LANDevice.1.WLANConfiguration.)
	for i in `seq 1 $max_num`;do
		freecwmp_parse_formated_parameter "$1$i." "InternetGatewayDevice.LANDevice.1.WLANConfiguration.{i}." "rc" "num"
		if [ $rc -eq 0 ] && [ $num -gt 0 ] && [ $num -le $max_num ]; then
			let uci_num=$num-1
			freecwmp_set_parameter_notification "InternetGatewayDevice.LANDevice.1.WLANConfiguration.$i." "0"
			freecwmp_set_parameter_notification "InternetGatewayDevice.LANDevice.1.WLANConfiguration.$i.Enable" "0"
			freecwmp_set_parameter_notification "InternetGatewayDevice.LANDevice.1.WLANConfiguration.$i.MaxBitRate" "0"
			freecwmp_set_parameter_notification "InternetGatewayDevice.LANDevice.1.WLANConfiguration.$i.Channel" "0"
			freecwmp_set_parameter_notification "InternetGatewayDevice.LANDevice.1.WLANConfiguration.$i.SSID" "0"
			freecwmp_set_parameter_notification "InternetGatewayDevice.LANDevice.1.WLANConfiguration.$i.BeaconType" "0"
			freecwmp_set_parameter_notification "InternetGatewayDevice.LANDevice.1.WLANConfiguration.$i.MACAddressControlEnabled" "0"
			freecwmp_set_parameter_notification "InternetGatewayDevice.LANDevice.1.WLANConfiguration.$i.Standard" "0"
			freecwmp_set_parameter_notification "InternetGatewayDevice.LANDevice.1.WLANConfiguration.$i.WEPKeyIndex" "0"
			freecwmp_set_parameter_notification "InternetGatewayDevice.LANDevice.1.WLANConfiguration.$i.KeyPassphrase" "0"
			freecwmp_set_parameter_notification "InternetGatewayDevice.LANDevice.1.WLANConfiguration.$i.WEPEncryptionLevel" "0"
			freecwmp_set_parameter_notification "InternetGatewayDevice.LANDevice.1.WLANConfiguration.$i.BasicEncryptionModes" "0"
			freecwmp_set_parameter_notification "InternetGatewayDevice.LANDevice.1.WLANConfiguration.$i.BasicAuthenticationMode" "0"
			freecwmp_set_parameter_notification "InternetGatewayDevice.LANDevice.1.WLANConfiguration.$i.WPAEncryptionModes" "0"
			freecwmp_set_parameter_notification "InternetGatewayDevice.LANDevice.1.WLANConfiguration.$i.WPAAuthenticationMode" "0"
			freecwmp_set_parameter_notification "InternetGatewayDevice.LANDevice.1.WLANConfiguration.$i.IEEE11iEncryptionModes" "0"
			freecwmp_set_parameter_notification "InternetGatewayDevice.LANDevice.1.WLANConfiguration.$i.IEEE11iAuthenticationMode" "0"
			freecwmp_set_parameter_notification "InternetGatewayDevice.LANDevice.1.WLANConfiguration.$i.RadioEnabled" "0"
			freecwmp_set_parameter_notification "InternetGatewayDevice.LANDevice.1.WLANConfiguration.$i.DeviceOperationMode" "0"
			freecwmp_set_parameter_notification "InternetGatewayDevice.LANDevice.1.WLANConfiguration.$i.AuthenticationServiceMode" "0"
			freecwmp_set_parameter_notification "InternetGatewayDevice.LANDevice.1.WLANConfiguration.$i.WEPKey.1.WEPKey" "0"
			freecwmp_set_parameter_notification "InternetGatewayDevice.LANDevice.1.WLANConfiguration.$i.WEPKey.2.WEPKey" "0"
			freecwmp_set_parameter_notification "InternetGatewayDevice.LANDevice.1.WLANConfiguration.$i.WEPKey.3.WEPKey" "0"
			freecwmp_set_parameter_notification "InternetGatewayDevice.LANDevice.1.WLANConfiguration.$i.WEPKey.4.WEPKey" "0"
			freecwmp_set_parameter_notification "InternetGatewayDevice.LANDevice.1.WLANConfiguration.$i.PreSharedKey.1.PreSharedKey" "0"
			freecwmp_set_parameter_notification "InternetGatewayDevice.LANDevice.1.WLANConfiguration.$i.PreSharedKey.1.KeyPassphrase" "0"
			freecwmp_set_parameter_notification "InternetGatewayDevice.LANDevice.1.WLANConfiguration.$i.PreSharedKey.1.AssociatedDeviceMACAddress" "0"
			fi
	done
	freecwmp_set_parameter_notification "$1" "$2"
	return $FAULT_CPE_NO_FAULT
	;;
	InternetGatewayDevice.LANDevice.1.WLANConfiguration.[1-$max_num].)
	freecwmp_parse_formated_parameter "$1" "InternetGatewayDevice.LANDevice.1.WLANConfiguration.{i}." "rc" "num"
	if [ $rc -eq 0 ] && [ $num -gt 0 ] && [ $num -le $max_num ]; then
		let uci_num=$num-1
		freecwmp_set_parameter_notification "InternetGatewayDevice.LANDevice.1.WLANConfiguration.$num.Enable" "0"
		freecwmp_set_parameter_notification "InternetGatewayDevice.LANDevice.1.WLANConfiguration.$num.MaxBitRate" "0"
		freecwmp_set_parameter_notification "InternetGatewayDevice.LANDevice.1.WLANConfiguration.$num.Channel" "0"
		freecwmp_set_parameter_notification "InternetGatewayDevice.LANDevice.1.WLANConfiguration.$num.SSID" "0"
		freecwmp_set_parameter_notification "InternetGatewayDevice.LANDevice.1.WLANConfiguration.$num.BeaconType" "0"
		freecwmp_set_parameter_notification "InternetGatewayDevice.LANDevice.1.WLANConfiguration.$num.MACAddressControlEnabled" "0"
		freecwmp_set_parameter_notification "InternetGatewayDevice.LANDevice.1.WLANConfiguration.$num.Standard" "0"
		freecwmp_set_parameter_notification "InternetGatewayDevice.LANDevice.1.WLANConfiguration.$num.WEPKeyIndex" "0"
		freecwmp_set_parameter_notification "InternetGatewayDevice.LANDevice.1.WLANConfiguration.$num.KeyPassphrase" "0"
		freecwmp_set_parameter_notification "InternetGatewayDevice.LANDevice.1.WLANConfiguration.$num.WEPEncryptionLevel" "0"
		freecwmp_set_parameter_notification "InternetGatewayDevice.LANDevice.1.WLANConfiguration.$num.BasicEncryptionModes" "0"
		freecwmp_set_parameter_notification "InternetGatewayDevice.LANDevice.1.WLANConfiguration.$num.BasicAuthenticationMode" "0"
		freecwmp_set_parameter_notification "InternetGatewayDevice.LANDevice.1.WLANConfiguration.$num.WPAEncryptionModes" "0"
		freecwmp_set_parameter_notification "InternetGatewayDevice.LANDevice.1.WLANConfiguration.$num.WPAAuthenticationMode" "0"
		freecwmp_set_parameter_notification "InternetGatewayDevice.LANDevice.1.WLANConfiguration.$num.IEEE11iEncryptionModes" "0"
		freecwmp_set_parameter_notification "InternetGatewayDevice.LANDevice.1.WLANConfiguration.$num.IEEE11iAuthenticationMode" "0"
		freecwmp_set_parameter_notification "InternetGatewayDevice.LANDevice.1.WLANConfiguration.$num.RadioEnabled" "0"
		freecwmp_set_parameter_notification "InternetGatewayDevice.LANDevice.1.WLANConfiguration.$num.DeviceOperationMode" "0"
		freecwmp_set_parameter_notification "InternetGatewayDevice.LANDevice.1.WLANConfiguration.$num.AuthenticationServiceMode" "0"
		freecwmp_set_parameter_notification "InternetGatewayDevice.LANDevice.1.WLANConfiguration.$i.WEPKey.1.WEPKey" "0"
		freecwmp_set_parameter_notification "InternetGatewayDevice.LANDevice.1.WLANConfiguration.$i.WEPKey.2.WEPKey" "0"
		freecwmp_set_parameter_notification "InternetGatewayDevice.LANDevice.1.WLANConfiguration.$i.WEPKey.3.WEPKey" "0"
		freecwmp_set_parameter_notification "InternetGatewayDevice.LANDevice.1.WLANConfiguration.$i.WEPKey.4.WEPKey" "0"
		freecwmp_set_parameter_notification "InternetGatewayDevice.LANDevice.1.WLANConfiguration.$i.PreSharedKey.1.PreSharedKey" "0"
		freecwmp_set_parameter_notification "InternetGatewayDevice.LANDevice.1.WLANConfiguration.$i.PreSharedKey.1.KeyPassphrase" "0"
