		let uci_num=$num-1
		set_wlan_key_passphrase $uci_num "$2"
	fi
	return $FAULT_CPE_NO_FAULT
	;;
	InternetGatewayDevice.LANDevice.1.WLANConfiguration.[1-$max_num].BeaconType)
	freecwmp_parse_formated_parameter "$1" "InternetGatewayDevice.LANDevice.1.WLANConfiguration.{i}.BeaconType" "rc" "num"
	if [ $rc -eq 0 ] && [ $num -gt 0 ] && [ $num -le $max_num ]; then
		let uci_num=$num-1
		set_wlan_beacon_type $uci_num "$2"
	fi
	return $FAULT_CPE_NO_FAULT
	;;
	InternetGatewayDevice.LANDevice.1.WLANConfiguration.[1-$max_num].WEPEncryptionLevel)
	return $FAULT_CPE_NOTIFICATION_REJECTED
	;;
	InternetGatewayDevice.LANDevice.1.WLANConfiguration.[1-$max_num].BasicEncryptionModes)
	freecwmp_parse_formated_parameter "$1" "InternetGatewayDevice.LANDevice.1.WLANConfiguration.{i}.BasicEncryptionModes" "rc" "num"
