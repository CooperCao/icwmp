get_management_server_password() {
local val=""
local permissions=""
local parm="InternetGatewayDevice.ManagementServer.Password"
case "$action" in
	get_value)
	# returned value must be empty
	val=""
	;;
	get_name)
	permissions="1"
	;;
	get_notification)
	freecwmp_get_parameter_notification "val" "$parm"
	;;
esac
freecwmp_output "$parm" "$val" "$permissions"
}

